			if(deger > PlayerData[playerid][pCash])
				return Dialog_Show(playerid, SirketKasaKoy, DIALOG_STYLE_INPUT, "Þirket Kasasi", "Þirket kasasina koymak istediginiz miktari giriniz.", "Ekle", "Kapat"), HataMesajGonder(playerid, "uzerinizde yeterli para yok.");

			sirketData[sirketid][sirketKasa] += deger;
