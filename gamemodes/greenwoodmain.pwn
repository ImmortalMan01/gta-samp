		    sayi++;
			format(string, sizeof(string), "%s{808080}%d\t{808080}%s\t{FF6347}Kiralanmis\t{808080}%d dakika\n", string, realid, ReturnVehicleModelName(AracInfo[i][aracModel]), AracInfo[i][aracKira]);
