forward BalikTutma(playerid);
public BalikTutma(playerid)
{
    new balikorani = 0, pYemTercihi;

	//if (PlayerData[playerid][pBalikOrani] > gettime()) balikorani += 15;

	if (PlayerData[playerid][pYemTurleri][KARIDES] >= 1)
	{
	    balikorani = 60; //maks viple 70
	    pYemTercihi = KARIDES;
	}

	if (PlayerData[playerid][pYemTurleri][KARIDES] == 0 && PlayerData[playerid][pYemTurleri][SULUNES] >= 1)
	{
	    balikorani = 50; //maks viple 65
        pYemTercihi = SULUNES;
	}
	if (PlayerData[playerid][pYemTurleri][KARIDES] == 0 && PlayerData[playerid][pYemTurleri][SULUNES] == 0 && PlayerData[playerid][pYemTurleri][EKMEK_KIRINTISI] >= 1)
	{
		balikorani = 40; //maks viple 50
        pYemTercihi = EKMEK_KIRINTISI;
	}

	switch (PlayerData[playerid][pVip])
	{
	    case 1: balikorani += 3;
	    case 2: balikorani += 7;
	    case 3: balikorani += 10;
	}

	if (random(100) < balikorani)
	{
	    new slot = GetPlayerFish_FreeSlot(playerid);

		if (slot == -1)
		{
		    BalikTimer[playerid] = -1;
		    RemovePlayerAttachedObject(playerid, GetPVarInt(playerid, "OltaSlot"));
			SetPVarInt(playerid, "OltaSlot", -1);
			ClearAnimations(playerid);
			TogglePlayerControllable(playerid, true);
			HataMesajGonder(playerid, "Daha fazla balik tutamazsýn.");
			return 1;
		}

		new id = RandomEx(0, sizeof(BalikTurleri)-1), gram = RandomEx(BalikTurleri[id][MinAgirlik], BalikTurleri[id][MaxAgirlik]);

		/*if (BoyleBalikVar(playerid, id))
		{
  		 	BalikTimer[playerid] = -1;
	     	RemovePlayerAttachedObject(playerid, GetPVarInt(playerid, "OltaSlot"));
		 	SetPVarInt(playerid, "OltaSlot", -1);
	     	ClearAnimations(playerid);
	     	TogglePlayerControllable(playerid, true);
	     	BilgiMesajGonder(playerid, "Balik yakalayamadiniz.");
	     	return 1;
		}*/

		if (PlayerData[playerid][pBalikOrani] > gettime()) balikorani += 15;

	    PlayerData[playerid][pYemTurleri][pYemTercihi] -= 1;

		PlayerData[playerid][pBalikID][slot] = id;
		PlayerData[playerid][pBalikGram][slot] = gram;

		MesajGonder(playerid, "Bir {FF0000}%s {FFFFFF}balýðý tuttunuz. {2ECC71}Gram: %d {FFFFFF}Balýkçýya giderek satabilirsin.",BalikTurleri[id][TurAdi],gram);

	 	BalikTimer[playerid] = -1;

   		RemovePlayerAttachedObject(playerid, GetPVarInt(playerid, "OltaSlot"));
		SetPVarInt(playerid, "OltaSlot", -1);
		ClearAnimations(playerid);
		TogglePlayerControllable(playerid, true);
	}
	else
	{
	     BalikTimer[playerid] = -1;
	     RemovePlayerAttachedObject(playerid, GetPVarInt(playerid, "OltaSlot"));
		 SetPVarInt(playerid, "OltaSlot", -1);
	     ClearAnimations(playerid);
	     TogglePlayerControllable(playerid, true);
	     BilgiMesajGonder(playerid, "Balik yakalayamadiniz.");
	}
	return 1;
}

