CMD:objeolustur(playerid, params[])
{
	new id, modelid;

	if (PlayerData[playerid][pAdmin] < 5)
	    return HataMesajGonder(playerid, "Yeterli yetkiniz yok!");

	if (sscanf(params, "d", modelid))
		return MesajGonder(playerid, "/objeolustur [Model ID]");

	id = Obje_Olustur(playerid, modelid);

	if (id == -1)
		return HataMesajGonder(playerid, "Obje olusturulurken bir hata olustu, lütfen gelistirici ile iletisime geciniz.");

	if (id != -1)
		BilgiMesajGonder(playerid, "Obje oluþturuldu ID %d.", id), AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d objeyi olusturdu.", PlayerData[playerid][pAdminName], id);

	return 1;
}

Obje_Olustur(playerid, modelid)
{
	new id = -1, Float:x, Float:y, Float:z, Float:ang;

	id = Iter_Free(Objeler);

	if (id != -1)
	{
		GetPlayerPos(playerid, x, y, z);
		GetPlayerFacingAngle(playerid, ang);

		x += (2.5 * floatsin(ang, degrees));
		y += (2.5 * floatcos(ang, degrees));

		Obje[id][objeID] = id;
		Obje[id][objeModelID] = modelid;
		Obje[id][objePos][0] = x;
	    Obje[id][objePos][1] = y;
	    Obje[id][objePos][2] = z;
	    Obje[id][objePos][3] = 0.0;
	    Obje[id][objePos][4] = 0.0;
	    Obje[id][objePos][5] = ang;
		Obje[id][objeData] = CreateDynamicObject(modelid, x, y, z, 0.0, 0.0, ang, -1, -1, -1, STREAMER_OBJECT_SD, STREAMER_OBJECT_DD, -1, 0);

		Iter_Add(Objeler, id);
		Obje[id][objeKullanimda] = true;
		mysql_tquery(g_SQL, "INSERT INTO `objeler` (`objeModel`) VALUES(0)", "ObjeYaratýldý", "d", id);
	}

	return id;
}
