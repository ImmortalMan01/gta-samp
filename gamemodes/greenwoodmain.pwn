Dialog:Giseler(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    switch (listitem)
	    {
	        case 0:
	        {
	            if (!AcilDurumMu())
	            {
		            AcilDurum = 1;

		            SendBirlikMessageEx(BIRLIK_LSPD, COLOR_DEPARTMENT, "* DEPARTMAN: Memur %s tarafindan butun giseler kilitlendi.", ReturnName(playerid, 0));
		            SendBirlikMessageEx(BIRLIK_FBI, COLOR_DEPARTMENT, "* DEPARTMAN: Memur %s tarafindan butun giseler kilitlendi.", ReturnName(playerid, 0));
	            }
	            else
	            {
	                AcilDurum = 0;

		            SendBirlikMessageEx(BIRLIK_LSPD, COLOR_DEPARTMENT, "* DEPARTMAN: Memur %s tarafindan butun giseler acildi.", ReturnName(playerid, 0));
		            SendBirlikMessageEx(BIRLIK_FBI, COLOR_DEPARTMENT, "* DEPARTMAN: Memur %s tarafindan butun giseler acildi.", ReturnName(playerid, 0));
	            }
	        }
	    }
	    GiseKontrolu(playerid);
	}
	return 1;
}

stock spamProtect(playerid, const szSpam[], iTime)
{
	new
		s_szPVar[32],
		s_iPVar
	;
	format(s_szPVar, sizeof(s_szPVar), "pv_iSpam_%s", szSpam);

	s_iPVar = GetPVarInt(playerid, s_szPVar);

	if((GetTickCount() - s_iPVar) < iTime * 1000) {
		return 0;
	} else {
		SetPVarInt(playerid, s_szPVar, GetTickCount());
	}
	return 1;
}

