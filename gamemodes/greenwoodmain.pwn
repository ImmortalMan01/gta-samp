public OnUnoccupiedVehicleUpdate(vehicleid, playerid, passenger_seat, Float:new_x, Float:new_y, Float:new_z, Float:vel_x, Float:vel_y, Float:vel_z)
{
    if(IsPlayerConnected(playerid))
    {
        new velocity;
        velocity = GetVehicleSpeedExHack(vel_x, vel_y, vel_z);
        if(IsVehicleValid(vehicleid))
        {
            if(!CheckDrive(vehicleid))
            {
                if(GetPVarInt(playerid, "Count") > 2)
                {
                    TogglePlayerControllable(playerid, false);
                    Kick(playerid);
                    AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, Car Fucker hilesi kullandigi için atýldý.", ReturnName(playerid, 0));
