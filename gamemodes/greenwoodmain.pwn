CMD:objekaldir(playerid, params[])
{
	new id, query[256];
	if (PlayerData[playerid][pAdmin] < 5)
	    return HataMesajGonder(playerid, "Yeterli yetkiniz yok!");

	if (sscanf(params, "d", id))
	    return BilgiMesajGonder(playerid, "/objekaldir [id]");

	if ((id < 0 || id >= MAX_OBJE) || !Obje[id][objeKullanimda]) return HataMesajGonder(playerid, "Geçersiz obje idsi.");

	DestroyDynamicObject(Obje[id][objeData]);
	Obje[id][objeData] = -1;
	MesajGonder(playerid, "Secilen obje basariyla silinmistir.");

	mysql_format(g_SQL, query, sizeof query, "DELETE FROM `objeler` WHERE `objeID` = '%d'", Obje[id][objeID]);
	mysql_query (g_SQL, query, false);

	Iter_Remove(Objeler, id);
	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu objeyi sildi.", PlayerData[playerid][pAdminName], id);
	return 1;
}


