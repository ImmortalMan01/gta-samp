        AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s hilesi kullandi, silahi silindi (onplayerweaponshot)", ReturnName(playerid), ReturnWeaponName(weaponid));
		SetWeapons(playerid);
        return 0;
    }

   	if (!GetPlayerWeapon(playerid))
    {
        AdminMessage(COLOR_LIGHTRED, "AdmLog: %s silah hilesi kullanmaya calistigi için sunucudan atýldý. (CODE-3)", ReturnName(playerid), ReturnWeaponName(GetPlayerWeapon(playerid)));
        Kick(playerid);
        return 0;
    }

    if (IsPlayerPause(playerid))
    {
        AdminMessage(COLOR_LIGHTRED, "AdmLog: %s silah hilesi kullanmaya calistigi için sunucudan atýldý. (CODE-4)", ReturnName(playerid), ReturnWeaponName(GetPlayerWeapon(playerid)));
        Kick(playerid);
        return 0;
    }

	if (IsBulletWeapon(weaponid) && GuvenliBolgedeyse(playerid) && GetFactionType(playerid) != BIRLIK_LSPD)
	{
	    SetPlayerArmedWeapon(playerid, 0);
