   	for (new i; i<14; i++) if (AracInfo[id][aracMods][i] > 0)
	{
	    CarComponents[vehicleid][i] = 0;
	}
    CarComponents[vehicleid][16] = -1;
    CarComponents[vehicleid][15] = -1;
    CarComponents[vehicleid][14] = -1;

    AracModifiye[vehicleid] = 1;
    PlayerData[playerid][pModifiye] = vehicleid;
	ModifiyeMenu(playerid);
	return 1;
}

