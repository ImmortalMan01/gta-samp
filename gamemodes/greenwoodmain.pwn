public OnVehicleMod(playerid, vehicleid, componentid)
{
	if (GetPlayerInterior(playerid) == 0)
	{
	    return Ban(playerid);
	}
	new id = Car_GetID(vehicleid);
	if(AracInfo[id][aracPaintjob] == -1) ChangeVehiclePaintjob(PlayerData[playerid][pModifiye], 3);
		else ChangeVehiclePaintjob(PlayerData[playerid][pModifiye], AracInfo[id][aracPaintjob]);

	new col1, col2;
	GetVehicleColor(vehicleid, col1, col2);
	for (new i = 0; i<14; i++)
	{
		if(GetVehicleComponentInSlot(vehicleid, i) != AracInfo[id][aracMods][i])
		{
			RemoveVehicleComponent(vehicleid, GetVehicleComponentInSlot(vehicleid, i));
			if (AracInfo[id][aracMods][i] > 0) AddVehicleComponent(vehicleid, AracInfo[id][aracMods][i]);
		}
	}
	if(col1 != AracInfo[id][aracRenkler][0] || col2 != AracInfo[id][aracRenkler][1])
	{
		ChangeVehicleColor(vehicleid, AracInfo[id][aracRenkler][0], AracInfo[id][aracRenkler][1]);
	}
    return 1;
}

