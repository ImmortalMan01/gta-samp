stock ReplaceAll(string[], size, const search[], const replace[])
{
    new pos;
    while ((pos = strfind(string, search, true)) != -1)
    {
        new temp[144];
        strmid(temp, string, 0, pos, size);
        strcat(temp, replace, size);
        strcat(temp, string[pos + strlen(search)], size);
        strmid(string, temp, 0, strlen(temp), size);
    }
}

