stock OOCMarket_UrunAl(playerid, urunadi[])
{
	new fiyat, durum;
	GetUrunBilgi(urunadi, fiyat, durum);
	if (strfind(urunadi, "isim/Cinsiyet Degistirme", true) != -1)
	{
