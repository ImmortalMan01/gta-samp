	if (Paintball[id][odaDurum] == ODA_BEKLEME) strcat(str, "{F21D1D}» Odayi Sil");
	SetPVarInt(playerid, "OdaAyarID", id);
	ShowPlayerDialog(playerid, ODA_AYARLARI, DIALOG_STYLE_TABLIST_HEADERS, "{1DF21D}Oda Ayarlari", str, "Seç", "Kapat");
	return 1;
}
