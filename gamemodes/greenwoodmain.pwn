        DestroyDynamicObject(PlayerData[playerid][pKampAtesiObject]);
        //MesajGonder(playerid, "Kamp Atesi kaldýrýldý.");
  	}
   	PlayerData[playerid][yayinKonuk] = -1;
   	if (PlayerData[playerid][pAramada] != 0)
