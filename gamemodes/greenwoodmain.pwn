GetXYInFrontOfPlayerBasketbol(playerid, &Float:x, &Float:y, Float:distance)
{
	new Float:a;
	GetPlayerPos(playerid, x, y, a);
	GetPlayerFacingAngle(playerid, a);
	if (GetPlayerVehicleID(playerid))
	{
	    GetVehicleZAngle(GetPlayerVehicleID(playerid), a);
	}
	x += (distance * floatsin(-a, degrees));
	y += (distance * floatcos(-a, degrees));
}

forward CheckPlayerArea(playerid);
public CheckPlayerArea(playerid)
{
    new Float:x, Float:y, Float:z;
    GetPlayerPos(playerid, x, y, z);

    if (x >= SAHA1MIN_X && x <= SAHA1MAX_X && y >= SAHA1MIN_Y && y <= SAHA1MAX_Y && z >= SAHA1MIN_Z && z <= SAHA1MAX_Z)
    {
        if (!IsInBasketballArea[playerid]) // Daha önce dýþarýdaysa
        {
            IsInBasketballArea[playerid] = true; // Þimdi içeride
            saha1 = 1;
            saha2 = 0;
            saha3 = 0;
        }
    }
    else
    {
        if(saha1 == 1)
    	{
	        if (IsInBasketballArea[playerid]) // Daha önce içerideyse
	        {
	            if(HavingBall[playerid])
				{
	            SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s topu sahanin ortasina paslar.", ReturnName(playerid, 0));
	            IsInBasketballArea[playerid] = false; // Þimdi dýþarýda
				    HavingBall[playerid] = 0;
				    if(animdurum[playerid] == true) return ClearAnim(playerid);

				    new Float:posX, Float:posY, Float:posZ;
				    GetPlayerPos(playerid, posX, posY, posZ);

				    new Float:dropX, Float:dropY;
				    GetXYInFrontOfPlayerBasketbol(playerid, dropX, dropY, 0.8);

				    StopObject(Ball);
				    MoveObject(Ball, dropX, dropY, posZ - 0.5, 2.5); // Topu yere býrakma

				    Baller = 999;
				    BallStatus = 0;
				    Anim[playerid] = 0;
				    BallBounce = 0;
				    MoveObject(Ball, SAHA1ORTA_X, SAHA1ORTA_Y, SAHA1ORTA_Z - 0.8, 5.0);
				    saha1 = 0;
	            	saha2 = 0;
	            	saha3 = 0;
				}
				if(!HavingBall[playerid])
				{
	            	IsInBasketballArea[playerid] = false; // Þimdi dýþarýda
				    HavingBall[playerid] = 0;
				    ClearAnim(playerid);
				    Baller = 999;
				    BallStatus = 0;
				    Anim[playerid] = 0;
				    BallBounce = 0;
				    saha1 = 0;
	            	saha2 = 0;
	            	saha3 = 0;
	            	if(animdurum[playerid] == true) return ClearAnim(playerid);
				}
	        }
        }
    }
    if (x >= SAHA2MIN_X && x <= SAHA2MAX_X && y >= SAHA2MIN_Y && y <= SAHA2MAX_Y && z >= SAHA2MIN_Z && z <= SAHA2MAX_Z)
    {
        if (!IsInBasketballArea[playerid]) // Daha önce dýþarýdaysa
        {
            IsInBasketballArea[playerid] = true; // Þimdi içeride
            saha1 = 0;
            saha2 = 1;
            saha3 = 0;
        }
    }
    else
    {
        if(saha2 == 1)
    	{
	        if (IsInBasketballArea[playerid]) // Daha önce içerideyse
	        {
	            if(HavingBall[playerid])
				{
	            SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s topu sahanin ortasina paslar.", ReturnName(playerid, 0));
	            IsInBasketballArea[playerid] = false; // Þimdi dýþarýda
				    HavingBall[playerid] = 0;
				    if(animdurum[playerid] == true) return ClearAnim(playerid);

				    new Float:posX, Float:posY, Float:posZ;
				    GetPlayerPos(playerid, posX, posY, posZ);

				    new Float:dropX, Float:dropY;
				    GetXYInFrontOfPlayerBasketbol(playerid, dropX, dropY, 0.8);

				    StopObject(saha2Ball);
				    MoveObject(saha2Ball, dropX, dropY, posZ - 0.5, 2.5); // Topu yere býrakma

				    Baller = 999;
				    BallStatus = 0;
				    Anim[playerid] = 0;
				    BallBounce = 0;
				    MoveObject(saha2Ball, SAHA2ORTA_X, SAHA2ORTA_Y, SAHA2ORTA_Z - 0.8, 5.0);
				    saha1 = 0;
	            	saha2 = 0;
	            	saha3 = 0;
				}
				if(!HavingBall[playerid])
				{
	            	IsInBasketballArea[playerid] = false; // Þimdi dýþarýda
				    HavingBall[playerid] = 0;
				    ClearAnim(playerid);
				    Baller = 999;
				    BallStatus = 0;
				    Anim[playerid] = 0;
				    BallBounce = 0;
				    saha1 = 0;
	            	saha2 = 0;
	            	saha3 = 0;
	            	if(animdurum[playerid] == true) return ClearAnim(playerid);
				}
	        }
        }
    }
    return 1;
}



