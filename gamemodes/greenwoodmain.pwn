CMD:setleader(playerid,params[])
{
	new pid,id;
	if (PlayerData[playerid][pAdmin] < 5) return HataMesajGonder(playerid, "Bu komutu kullanamazsin.");
	if (sscanf(params, "ui",pid,id)) return BilgiMesajGonder(playerid, "/setleader [ID/Isim] [Birlik ID] (-1 yazarsan liderlikten atilir)");
	if (!OyundaDegil(pid)) return HataMesajGonder(playerid, "Oyuncu oyunda deðil!");
	if ((id < -1 || id >= MAX_BIRLIK) || (id != -1 && !Birlikler[id][birlikExists])) return HataMesajGonder(playerid, "Hatali Birlik ID.");
	if (id == -1)
	{
	    BirliktenAt(pid);
	    BilgiMesajGonder(playerid, "%s adlý oyuncuyu birlik liderliginden attiniz.", ReturnName(pid, 0));
    	BilgiMesajGonder(pid, "%s adlý yetkili sizi birlik liderliginden atti.", ReturnName(playerid, 0));
    	Oyuncu_Kaydet(id);
    	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý oyuncuyu birlik liderliginden çýkardý", PlayerData[playerid][pAdminName], ReturnName(pid, 0));
	}
	else
	{
	    BirliktenAt(pid);
	    PlayerData[pid][pFaction] = id;
	    PlayerData[pid][pFactionRutbe] = Birlikler[id][birlikRutbeler];
	    PlayerData[pid][pFactionDivizyon] = 0;
	    BilgiMesajGonder(playerid, "%s adlý oyuncuyu \"%s\" adlý birligin lideri yaptiniz.", ReturnName(pid, 0),Birlikler[id][birlikAd]);
	    BilgiMesajGonder(pid, "%s adlý yetkili seni \"%s\" adlý birligin lideri yapti.", ReturnName(playerid, 0),Birlikler[id][birlikAd]);
	    AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý oyuncuyu %s birliginin lideri yapti.", PlayerData[playerid][pAdminName], ReturnName(pid, 0), Birlikler[id][birlikAd]);
	    Oyuncu_Kaydet(pid);
	}
	return 1;
}

CMD:isyeriolustur(playerid,params[])
{
	new tip,fiyat;
	if (PlayerData[playerid][pAdmin] < 7) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok");
	if (sscanf(params, "dd", tip, fiyat))
	{
		BilgiMesajGonder(playerid, "/isyeriolustur [Tip] [Fiyat]");
		SendClientMessage(playerid,-1, "{00FF15}[TIP]: {FFFFFF} 1: Market 2: Kiyafet Magazasi 3: Fast Food 4: Galeri 5: Elektronik 6: Bar/Pub 7: Kumarhane 8:Ofis");
		return 1;
	}
	if (tip < 1 || tip > 8) return HataMesajGonder(playerid, "Geçersiz isyeri tipi. 1 ile 8 arasinda olmalýdýr.");

	new id;
	id = Isyeri_Olustur(playerid, tip, fiyat);

	if (id == -1)
		return HataMesajGonder(playerid, "Sunucu isyeri sinirina ulasti.");

	BilgiMesajGonder(playerid, "Ýþyeri oluþturuldu, ID: %d",id);
	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerini olusturdu.", PlayerData[playerid][pAdminName], id);
	return 1;
}

CMD:isyerisil(playerid, params[])
{
	static
	    id = 0;

    if (PlayerData[playerid][pAdmin] < 7)
	    return HataMesajGonder(playerid, "Bu komutu kullanamazsin !");

	if (sscanf(params, "d", id))
	    return BilgiMesajGonder(playerid, "/isyerisil [isyeri ID]");

	if ((id < 0 || id >= MAX_ISYERI) || !Isyeri[id][isyeriExists])
	    return HataMesajGonder(playerid, "Geçersiz isyeri ID'si.");

	Isyeri_Sil(id);
	BilgiMesajGonder(playerid, "Ýþyeri silindi, ID: %d.", id);
	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerini sildi.", PlayerData[playerid][pAdminName], id);
	return 1;
}
CMD:isyeriduzenle(playerid,params[])
{
	static id, type[24], string[128];
	if (PlayerData[playerid][pAdmin] < 7) return HataMesajGonder(playerid, "Bu komutu kullanamazsin !");
	if (sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	BilgiMesajGonder(playerid, "/isyeriduzenle [ID] [Ad]");
	    SendClientMessage(playerid, COLOR_YELLOW, "{00FF15}[ADLAR]: {FFFFFF} pozisyon, interior, fiyat, stok, tip, spawn, devlet, aracekle, aracdegistir, vergi, ooc");
		return 1;
	}
	if ((id < 0 || id >= MAX_ISYERI) || !Isyeri[id][isyeriExists])
	    return HataMesajGonder(playerid, "Geçersiz isyeri ID'si.");

	if (!strcmp(type, "aracekle", true))
	{
		Dialog_Show(playerid,AracEkle,DIALOG_STYLE_INPUT, "{2ECC71}Arac Ekle", "Lütfen iþyerine eklemek istediginiz aracýn adini veya modelini girin:", "Devam", "Kapat");
	}

	if (!strcmp(type, "aracdegistir", true))
	{
		Dialog_Show(playerid,AracDuzenle,DIALOG_STYLE_LIST, "{2ECC71}Arac Duzenle", "{FFFFFF}Arac Fiyat Degistir\n{FFFFFF}Arac Sil", "Seç", "Geri");
	}

	if (!strcmp(type, "pozisyon", true))
	{
	    GetPlayerPos(playerid, Isyeri[id][isyeriPos][0], Isyeri[id][isyeriPos][1], Isyeri[id][isyeriPos][2]);
		GetPlayerFacingAngle(playerid, Isyeri[id][isyeriPos][3]);

		Isyeri_Yenile(id);
		Isyeri_Kaydet(id);
		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerinin pozisyonunu degistirdi.", PlayerData[playerid][pAdminName], id);
	}

 	else if (!strcmp(type, "ooc", true))
    {
        new fiyat;

	    if (sscanf(string, "d", fiyat))
	        return BilgiMesajGonder(playerid, "/isyeriduzenle [ID] [ooc] [ooc fiyat] (0 ooc satisi devredisi birakir)");

		Isyeri[id][isyeriOOC] = fiyat;
		Isyeri_Yenile(id);
		Isyeri_Kaydet(id);
		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerinin ooc fiyatini %dbCoin yapti.", PlayerData[playerid][pAdminName], id, fiyat);
    }

	else if (!strcmp(type, "interior", true))
	{
	    GetPlayerPos(playerid, Isyeri[id][isyeriInt][0], Isyeri[id][isyeriInt][1], Isyeri[id][isyeriInt][2]);
		GetPlayerFacingAngle(playerid, Isyeri[id][isyeriInt][3]);

		Isyeri[id][isyeriInterior] = GetPlayerInterior(playerid);

        foreach (new i : Player)
		{
			if (PlayerData[i][pIsyeri] == Isyeri[id][isyeriID])
			{
				SetPlayerPosForAC(i, Isyeri[id][isyeriInt][0], Isyeri[id][isyeriInt][1], Isyeri[id][isyeriInt][2]);
				SetPlayerFacingAngle(i, Isyeri[id][isyeriInt][3]);

				SetPlayerInterior(i, Isyeri[id][isyeriInterior]);
				SetCameraBehindPlayer(i);
			}
		}
		Isyeri_Kaydet(id);
		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerinin interiorunu degistirdi.", PlayerData[playerid][pAdminName], id);
		BilgiMesajGonder(playerid, "%d ID'li isyerinin interioru su an bulundugunuz interior (ID: %d) olarak degistirildi.",id,GetPlayerInterior(playerid));
	}
	else if (!strcmp(type, "fiyat", true))
	{
	    new price;

	    if (sscanf(string, "d", price))
	        return BilgiMesajGonder(playerid, "/isyeriduzenle [ID] [Fiyat] [Yeni Fiyat]");

	    Isyeri[id][isyeriFiyat] = price;
	    Isyeri[id][isyeriKendiFiyati] = price;

	    Isyeri_Yenile(id);
	    Isyeri_Kaydet(id);

		BilgiMesajGonder(playerid, "%d ID'li isyerinin fiyati {2ECC71}%s {FFFFFF}olarak degistirildi.",id,FormatNumber(price));
		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d nolu isyerinin fiyatini %d$ yapti.", PlayerData[playerid][pAdminName], id, price);
	}
	else if (!strcmp(type, "stok", true))
	{
	    new urunid,amount;

	    if (sscanf(string, "dd",urunid,amount))
	        return BilgiMesajGonder(playerid, "/isyeriduzenle [ID] [stok] [urun ID] [urun Sayisi]");

