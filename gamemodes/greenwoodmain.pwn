CMD:beanbag(playerid)
{
    if (!OyundaDegil(playerid)) return 1;
    if (GetFactionType(playerid) != BIRLIK_LSPD) return HataMesajGonder(playerid, "Devlet memuru deðilsin.");
    if (!IsPlayerInAnyVehicle(playerid)) return HataMesajGonder(playerid, "Polis aracinda deðilsin!");
    new vehicleid = GetPlayerVehicleID(playerid);
    new id = Car_GetID(vehicleid);
    if (id == -1 || AracInfo[id][aracFactionType] != BIRLIK_LSPD) return HataMesajGonder(playerid, "Polis aracinda deðilsin!");
    if (!PlayerData[playerid][pBeanbag])
    {
        if (Beanbag[vehicleid] != 1) return HataMesajGonder(playerid, "Bu aracta Beanbag yok.");
        PlayerData[playerid][pBeanbag] = 1;
        SaveWeapons(playerid);
        Beanbag[vehicleid] = 0;
        GivePlayerWeapon(playerid, 25, 100);
        SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s aracýn orta panelinden beanbag tufegini eline alýr.", ReturnName(playerid, 0));
    }
    else
    {
        if (Beanbag[vehicleid] != 0) return HataMesajGonder(playerid, "Bu aracta zaten Beanbag var.");
        ResetPlayerWeapons(playerid);
        PlayerData[playerid][pBeanbag] = 0;
        SetWeapons(playerid);
        Beanbag[vehicleid] = 1;
        SetPlayerArmedWeapon(playerid, 0);
       	SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s beanbag tufegini aracýn orta paneline yerlestirir.", ReturnName(playerid, 0));
    }
	return 1;
}

CMD:adminck(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 7) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok!");

	new oyuncuadi[32];
	if (sscanf(params, "s[32]", oyuncuadi)) return BilgiMesajGonder(playerid, "/adminck [Ad_Soyad]");
	new oyuncuid = OyuncuVarKontrol(oyuncuadi);
	if(IsPlayerConnected(OyuncuVarKontrol(oyuncuadi)))
	{
	    ResetPlayer(oyuncuid);
    	ResetWeapons(oyuncuid);
    	ClearAnimations(oyuncuid);
