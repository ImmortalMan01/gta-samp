//antihile
stock GetVehicleSpeedExHack(Float:x, Float:y, Float:z)
{
    new vel;
    vel = floatround(floatsqroot( x*x + y*y + z*z ) * 180); // KM/H
    return vel;
}

stock IsVehicleValid(vehicleid)
{
    if(vehicleid != INVALID_VEHICLE_ID) return 1;
    else return 0;
}

stock GetVehicleSpeedExHackk(Float:x, Float:y, Float:z)
{
    new vel;
    vel = floatround(floatsqroot( x*x + y*y + z*z ) * -20); // KM/H
    return vel;
}

stock CheckDrive(vehicleid)
{
    foreach(Player, i){
        if(IsPlayerInAnyVehicle(i)){
            if(GetPlayerVehicleID(i) == vehicleid){
                if(GetPlayerState(i) == PLAYER_STATE_DRIVER){
                    return 1;
                }
            }
        }
    }
    return 0;
}

const IPacket_DriverSync = 200;
const IPacket_OnFootSync = 207;
const IPacket_PassengerSync = 211;
const UNOCCUPIED_SYNC = 209;

#if !defined IsNaN
    #define IsNaN(%0) ((%0) != (%0))
#endif

public OnIncomingPacket(playerid, packetid, BitStream:bs)
{
	switch(packetid)
	{
		case IPacket_OnFootSync:
		{
 			new BWH_OnFootData[PR_OnFootSync];

        	BS_IgnoreBits(bs, 8);
        	BS_ReadOnFootSync(bs, BWH_OnFootData);
			BS_ResetReadPointer(bs);

   			new Float:player_speed;
	        player_speed = floatsqroot(floatpower(floatabs(BWH_OnFootData[PR_velocity][0]), 2.0) +
	        floatpower(floatabs(BWH_OnFootData[PR_velocity][1]), 2.0) +
	        floatpower(floatabs(BWH_OnFootData[PR_velocity][2]), 2.0)) * 179.28625;

			if(GirisYaptimi[playerid] == false)
			{
		    	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s spawn bugu yaptigi için kicklendi.", ReturnName(playerid));
			    Kick(playerid);
			}
         	if(player_speed < 27.0)
         	{
         		if(__scy_ab_POS_X[playerid] != 0.0)
         		{
         			if(BWH_OnFootData[PR_surfingVehicleId] == 0)
         			{
           				if(BWH_OnFootData[PR_position][0] > __scy_ab_POS_X[playerid] + 1.3 || BWH_OnFootData[PR_position][1] > __scy_ab_POS_Y[playerid] + 1.3 || BWH_OnFootData[PR_position][2] > __scy_ab_POS_Z[playerid] + 1.3
                    	|| BWH_OnFootData[PR_position][0] < __scy_ab_POS_X[playerid] - 1.3 || BWH_OnFootData[PR_position][1] < __scy_ab_POS_Y[playerid] - 1.3
                    	|| BWH_OnFootData[PR_position][2] < __scy_ab_POS_Z[playerid] - 1.3)
                    	{
            				if((gettime() - __scy_ab__LAST_CALL[playerid]) < 2 && __scy_ab__LAST_CALL[playerid] != 0 && GetPVarInt(playerid, "KOSKorumasi") == 0 && !IsPlayerPause(playerid) && PlayerData[playerid][pDragged] == 0)
                            {
                                AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, airbreak kullaniyor olabilir.", ReturnName(playerid, 0));
                                new id, isim[MAX_PLAYER_NAME];
   					new DCC_Embed:ixd = DCC_CreateEmbed(), str[300], gonderen[64];


					cache_get_value_name_int(0, "ID", id);
					cache_get_value_name(0, "Isim", isim, MAX_PLAYER_NAME);

					format(gonderen, sizeof(gonderen), "*%s*", ReturnName(id, 0));
					DCC_SetEmbedColor(ixd, 3066993);

   					format(str, sizeof(str), "adli oyuncu `airbreak` hilesi kullaniyor olabilir.");
                    DCC_AddEmbedField(ixd, gonderen, str);
					DCC_SendChannelEmbedMessage(DCC_FindChannelById("1350589048166682644"), ixd);
							}
							__scy_ab__LAST_CALL[playerid] = gettime();
   						}
                	}
           		}
        	}

	        __scy_ab_POS_X[playerid] = BWH_OnFootData[PR_position][0];
