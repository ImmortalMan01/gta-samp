Dialog:Yas(playerid, response, listitem, inputtext[])
{
	if(!response)
	    return Kick(playerid);
	if(response)
	{
        if (!IsNumeric(inputtext) || strval(inputtext) < 18 || strval(inputtext) > 80)
        {
         	return Dialog_Show(playerid, Yas, DIALOG_STYLE_INPUT, "{38c5f7}Yasiniz", "{FFFFFF}Lütfen karakterinizin yasini girin.\n{FFFFFF}Yasiniz 18 ile 80 yas arasinda olmalýdýr.", "Devam", "Iptal");
        }
        PlayerData[playerid][pYas] = strval(inputtext);

        Dialog_Show(playerid, Cinsiyet, DIALOG_STYLE_MSGBOX, "{38c5f7}Cinsiyet", "{FFFFFF}Lütfen karakterinizin cinsiyetini belirleyin.", "Erkek", "Kadin");
	}
	return 1;
}

