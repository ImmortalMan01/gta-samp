stock trcar1(const input[], output[])
{
    new i = 0;
    while (input[i] != '\0')
    {
        switch (input[i])
        {
            case 'ç': output[i] = 'c';
            case 'Ç': output[i] = 'C';
            case 'ð': output[i] = 'g';
            case 'Ð': output[i] = 'G';
            case 'ý': output[i] = 'i';
            case 'Ý': output[i] = 'I';
            case 'ö': output[i] = 'o';
            case 'Ö': output[i] = 'O';
            case 'þ': output[i] = 's';
            case 'Þ': output[i] = 'S';
            case 'ü': output[i] = 'u';
            case 'Ü': output[i] = 'U';
            default: output[i] = input[i];
        }
        i++;
    }
    output[i] = '\0'; // Stringi sonlandir
}

