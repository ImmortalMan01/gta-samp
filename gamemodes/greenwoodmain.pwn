CMD:agacolustur(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 6) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok.");
	new id = Iter_Free(Trees);
	if (id == -1) return SendClientMessage(playerid, 0xE74C3CFF, "Daha fazla agac olusturulamaz.");
 	new Float: x, Float: y, Float: z, Float: a;
 	GetPlayerPos(playerid, x, y, z);
 	GetPlayerFacingAngle(playerid, a);
 	x += (3.0 * floatsin(-a, degrees));
	y += (3.0 * floatcos(-a, degrees));
	z -= 1.0;

	TreeData[id][treeX] = x;
	TreeData[id][treeY] = y;
	TreeData[id][treeZ] = z;
	TreeData[id][treeRX] = TreeData[id][treeRY] = TreeData[id][treeRZ] = 0.0;

	TreeData[id][treeObjID] = CreateDynamicObject(657, TreeData[id][treeX], TreeData[id][treeY], TreeData[id][treeZ], TreeData[id][treeRX], TreeData[id][treeRY], TreeData[id][treeRZ]);

	new label[96];
