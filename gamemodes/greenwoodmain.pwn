CMD:silahban(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 6) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok!");

	new oyuncuadi[32], gun;
	if (sscanf(params, "ds[32]", gun, oyuncuadi)) return BilgiMesajGonder(playerid, "/silahban [Gun] [Ad_Soyad] (0 yazarsaniz sýfýrlanýr.)");
	new oyuncuid = OyuncuVarKontrol(oyuncuadi);
	if(IsPlayerConnected(OyuncuVarKontrol(oyuncuadi)))
	{
	    if(gun == 0)
	    {
		    AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþinin silah banýný kaldirdi.", PlayerData[playerid][pAdminName], ReturnName(oyuncuid, 0));
		    BilgiMesajGonder(oyuncuid, "%s adlý yetkili silah banýný kaldirdi.", PlayerData[playerid][pAdminName]);
		    PlayerData[oyuncuid][pSilahban] = 0;
		    Oyuncu_Kaydet(oyuncuid);
	    }
	    else
		{
		    foreach (new i:Player) if (PlayerData[i][pAyarlar][3] == 0 || i == oyuncuid)
		    {
	     		SendClientMessageEx(i, COLOR_LIGHTRED, "(%s) - (( %s adlý oyuncuya %s tarafindan %d gunluk silah bani uygulandi.))", ReturnDate(), ReturnName(oyuncuid, 0),  PlayerData[playerid][pAdminName], gun);
		    }
		    AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþiye %d gunluk silah bani uyguladi.", PlayerData[playerid][pAdminName], ReturnName(oyuncuid, 0), gun);
		    HataMesajGonder(oyuncuid, "%s adlý yetkili sana %d gunlugune silah bani uyguladi bu surede silah kullanamayacaksin.", PlayerData[playerid][pAdminName], gun);
		    PlayerData[oyuncuid][pSilahban] = gettime()+ (gun*86400);
		    Oyuncu_Kaydet(oyuncuid);
		}
	}
	if(!IsPlayerConnected(OyuncuVarKontrol(oyuncuadi)))
	{
 		if(gun == 0)
	    {
			new Cache:VeriCek, rows, query[256];
			mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM oyuncular WHERE ID = %d", IsimdenSQLID(oyuncuadi));
			VeriCek = mysql_query(g_SQL, query);
			rows = cache_num_rows();
			if(rows)
			{
	  			new querya[1024];
				format(querya,sizeof(querya), "SELECT `oyuncular` SET `Silahban` = '0' WHERE `ID` = '%d'", IsimdenSQLID(oyuncuadi));
				mysql_query(g_SQL,querya);

				AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþinin silah banýný offline olarak kaldirdi.", PlayerData[playerid][pAdminName], oyuncuadi);
			}
			else
			{
			    HataMesajGonder(playerid, "Böyle bir karakter bulunamadi.");
			}
			cache_delete(VeriCek);
	    }
	    else
	    {
			new Cache:VeriCek, rows, query[256];
			mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM oyuncular WHERE ID = %d", IsimdenSQLID(oyuncuadi));
			VeriCek = mysql_query(g_SQL, query);
			rows = cache_num_rows();
			if(rows)
			{
	  			new querya[1024];
				format(querya,sizeof(querya), "SELECT `oyuncular` SET `Silahban` = '%d' WHERE `ID` = '%d'", gettime()+ (gun*86400), IsimdenSQLID(oyuncuadi));
				mysql_query(g_SQL,querya);

				foreach (new i:Player) if (PlayerData[i][pAyarlar][3] == 0)
				{
  					SendClientMessageEx(i, COLOR_LIGHTRED, "(%s) - (( %s adlý oyuncuya offline olarak %s tarafindan %d gunluk silah bani uygulandi.))", ReturnDate(), oyuncuadi, PlayerData[playerid][pAdminName], gun);
				}
				AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþiye offline olarak %d gunluk silah bani uyguladi.", PlayerData[playerid][pAdminName], oyuncuadi, gun);
			}
			else
			{
			    HataMesajGonder(playerid, "Böyle bir karakter bulunamadi.");
			}
			cache_delete(VeriCek);
		}
	}
	return 1;
}

