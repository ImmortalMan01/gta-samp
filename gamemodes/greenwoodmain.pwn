CMD:sirketduzenle(playerid, params[])
{
	new sirket, type[18], amount[32]; // 2 film 3 otomativ 1 oyun
	if(PlayerData[playerid][pAdmin] < 8) return HataMesajGonder(playerid, "Yeterli yetkiniz yok.");
	if(sscanf(params, "us[18]s[32]", sirket, type, amount))
	{
	    BilgiMesajGonder(playerid, "/sirketduzenle [Sirket ID] [Adlar]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[ADLAR]:{FFFFFF} isim, sahip, hak, vergi, vergizaman, calisma, maxcalisma, isim, tip, kasa");
		return 1;
	}
	if(sirketData[sirket][sirketExists] == 0) return HataMesajGonder(playerid, "Böyle bir sirket yok.");

	if(!strcmp(type, "isim", true))
	{
	    if(isnull(amount)) return BilgiMesajGonder(playerid, "/sirketduzenle [ID] [isim] [Isim]");
	    format(sirketData[sirket][sirketAd], 24, amount);
	    MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
	    Sirket_Kaydet(sirket);
	    return 1;
	}
	if(!strcmp(type, "sahip", true))
	{
		if(isnull(amount) || IsimdenSQLID(amount) == -1) return HataMesajGonder(playerid, "Böyle bir oyuncu Veritabaninda yok.");
		sirketData[sirket][sirketSahip] = IsimdenSQLID(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
	}
	if(!strcmp(type, "vergi", true))
	{
		if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen sayi giriniz.");
		sirketData[sirket][sirketVergi] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	if(!strcmp(type, "vergizaman", true))
	{
		if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen dakika biciminden kac dakika sonra olmasini istiyorsaniz girin.");
		sirketData[sirket][sirketVergiZaman] = gettime() + strval(amount)*60;
		//sirketData[sirket][sirketVergiKalan] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	if(!strcmp(type, "calisma", true))
	{
		if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen sayi giriniz.");
		sirketData[sirket][sirketCP] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	if(!strcmp(type, "hak", true))
	{
	    if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen sayi giriniz.");
	    sirketData[sirket][sirketHak] = strval(amount);
	    MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
	    Sirket_Kaydet(sirket);
	    return 1;
	}
	if(!strcmp(type, "maxcalisma", true))
	{
		if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen sayi giriniz.");
		sirketData[sirket][sirketCPT] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
        return 1;
	}
	if(!strcmp(type, "isim", true))
	{
		if(strlen(amount) > 24) return HataMesajGonder(playerid, "isim en fazla 24 karakter olabilir.");
		format(sirketData[sirket][sirketAd], 24, amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	if(!strcmp(type, "tip", true))
	{
		if(strval(amount) != 1 && strval(amount) != 2) return HataMesajGonder(playerid, "1 = Oyun, 2 = Film");
		sirketData[sirket][sirketTip] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	if(!strcmp(type, "kasa", true))
	{
		if(isnull(amount) || !IsNumeric(amount)) return HataMesajGonder(playerid, "Lütfen sayi giriniz.");
		sirketData[sirket][sirketKasa] = strval(amount);
		MesajGonder(playerid, "Degisiklikler basariyla kaydedildi.");
		Sirket_Kaydet(sirket);
		return 1;
	}
	return 1;
}

CMD:sirketkur(playerid,params[])
{
    new sirketid = PlayerData[playerid][pSirket];
    if(sirketid != -1) return HataMesajGonder(playerid, "Þu anda zaten bir sirkettesin");
    Dialog_Show(playerid, SirketKur, DIALOG_STYLE_INPUT, "> sirket Kurma Menusu", "*** Þirket Kurma Þartkalý\n\n* En az 5 seviye olmanýz gerekmektedir.\n* $15.500'e sahip olmanýz gerekmektedir.\n\nEger bu sartlara uyuyorsan sirketinin adini gir;", "Kur", "Iptal");
    return 1;
}

CMD:sirket(playerid, params[])
{
    new sirketid = PlayerData[playerid][pSirket];

    if (sirketid == -1) return HataMesajGonder(playerid, "Þu anda hiçbir bir sirkete üye deðilsin.");
    new
                hours,
                minutes,
                seconds;

    new string[1500], toplamoyuncu[1024];
    strcat(string, "Ýslev\t#\n");
    new toplamsayi;
    foreach(new i : Player)
    {
        if(sirketid == PlayerData[i][pSirket]) toplamsayi++;
    }
    new tip[64];
        switch (sirketData[sirketid][sirketTip]) {
            case 1: tip = "Oyun Studyosu";
            case 2: tip = "Film Studyosu";
        }
    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Þirket Tipi\t{FFFFFF}%s {EEDD82}\n", tip);
    strcat(string, toplamoyuncu);
    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Þirket Yönetim Kurulu Baþkaný\t{22AF1D}%s {FFFFFF}\n", SQLName(sirketData[sirketid][sirketSahip]));
    strcat(string, toplamoyuncu);
    GetElapsedTime(sirketData[sirketid][sirketVergiZaman], hours, minutes, seconds);
    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Gelecek Vergi\t{EEDD82}{FFFFFF}%02d saat %02d dakika{22AF1D}(%s)\n", hours, minutes, FormatNumber(sirketData[sirketid][sirketVergi]));
    strcat(string, toplamoyuncu);
    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Þirket Çalýþma Potansiyeli\t{EEDD82}{FFFFFF}%d/%d saat\n", sirketData[sirketid][sirketCP],sirketData[sirketid][sirketCPT]);
    strcat(string, toplamoyuncu);
    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Çalýþanlar\n");
    strcat(string, toplamoyuncu);


    if(sirketData[sirketid][sirketTip] == 1){
    	format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Oyun Yayýnlama Hakký\t{00ff00}%d\n", sirketData[sirketid][sirketHak]);
    	strcat(string, toplamoyuncu);
    	strcat(string, "{EEDD82}»{FFFFFF} Oyun Studyosu\n");
    	strcat(string, "{EEDD82}»{FFFFFF} Geçmiþ Oyunlar\n");
    }
    if(sirketData[sirketid][sirketTip] == 2){
    	format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Fim Yayýnlama Hakký\t{00ff00}%d\n", sirketData[sirketid][sirketHak]);
    	strcat(string, toplamoyuncu);
    	strcat(string, "{EEDD82}»{FFFFFF} Film Studyosu\n");
    	strcat(string, "{EEDD82}»{FFFFFF} Geçmiþ Filmler\n");
    }

    if(PlayerData[playerid][pID] == sirketData[sirketid][sirketSahip])
    {
	    format(toplamoyuncu, sizeof(toplamoyuncu), "{EEDD82}»{FFFFFF} Çalýþan Al\n");
