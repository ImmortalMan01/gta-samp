}

CMD:agacduzenle(playerid, params[])
{
    if (PlayerData[playerid][pAdmin] < 6) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok.");
    if (EditingTreeID[playerid] != -1) return HataMesajGonder(playerid, "Zaten agac editliyorsun.");
	new id;
	if (sscanf(params, "i", id)) return BilgiMesajGonder(playerid, "/agacduzenle [tree id]");
