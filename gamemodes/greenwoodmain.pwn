		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d idli tamirhanenin tamir suresini %dsn olarak ayarladi.", PlayerData[playerid][pAdminName], id, vakit);
		Tamirhane_Kaydet(id, true);
		return 1;
	}
	else if(!strcmp(operation, "birlik", true))
	{
		new birlik;

		if(sscanf(str, "i", birlik))
		{
			BilgiMesajGonder(playerid, "/tamirduzenle [%d] [birlikid -1 birliksiz yapar]", id);
			return 1;
		}

	    if ((birlik < -1 || birlik >= MAX_BIRLIK) || (birlik != -1 && !Birlikler[id][birlikExists]))
