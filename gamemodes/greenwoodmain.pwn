CMD:gasp(playerid, params[])
{
	if (!OyundaDegil(playerid)) return 1;
	if (PlayerData[playerid][pKelepce] > 0 || PlayerData[playerid][pHapisSure] > 0 || PlayerData[playerid][pSoklandi] > 0 || PlayerData[playerid][pYereYatirildi] > 0 || PlayerData[playerid][pBaygin] == 1 || pbOda[playerid] != -1 || GetPVarInt(playerid, "IpBaglandi") == 1) return HataMesajGonder(playerid, "Bu komutu kullanamazsýnýz.");
	if (PlayerData[playerid][pLevel] < 5) return HataMesajGonder(playerid, "Gasp sistemini 5 seviye ustu oyuncular kullanabilir.");
	//if (PlayerData[playerid][pLevel] < 999999) return HataMesajGonder(playerid, "Sistem pasif.");
	if (PlayerData[playerid][pGaspSure] > gettime())
	{
	    new secs = PlayerData[playerid][pGaspSure] - gettime();
	    new saat = floatround(secs / 3600);
	    secs = secs - saat*3600;
	    new dakika = floatround(secs / 60);
	    secs = secs - dakika*60;
		return HataMesajGonder(playerid, "Suan gasp edemezsiniz. [Kalan %d saat %d dakika]", saat, dakika);
	}
	if(PlayerData[playerid][pProbation] == 1) return HataMesajGonder(playerid, "Probationdaki oyuncular bunu yapamaz.");
	new userid;
	if (sscanf(params, "u", userid)) return BilgiMesajGonder(playerid, "/gasp [Oyuncuid]");
	if (!OyundaDegil(userid)) return HataMesajGonder(playerid, "Böyle bir oyuncu yok.");
	if (userid == playerid) return HataMesajGonder(playerid, "Kendinizi gasp edemezsiniz.");
	if (PlayerData[userid][pLevel] < 5) return HataMesajGonder(playerid, "3 Level assagisindaki birisini gasp edemezsiniz.");
	//if (userid == playerid) return HataMesajGonder(playerid, "Kendinizi gasp edemezsiniz.");
	if (PlayerData[userid][pGaspEden] != INVALID_PLAYER_ID) return HataMesajGonder(playerid, "Bu oyuncuyu baþkasý gasp ediyor.");
	new Float:x, Float:y, Float:z;
 	GetPlayerPos(userid, x, y, z);
 	if (!IsPlayerInRangeOfPoint(playerid, 5.0, x,y,z)) return HataMesajGonder(playerid, "Bu oyunucuya yakin deðilsiniz!");
	if(PlayerData[userid][pProbation] == 1) return HataMesajGonder(playerid, "Gasp etmek istediginiz kiþi probationda.");
	if(PlayerData[userid][pGaspEdilmeSure] > gettime())

	PlayerData[userid][pGaspEden] = playerid;
	new strr[300];
	format(strr, 300, "{ffffff}%s {cccccc}adlý oyuncu sizi gasp etmek istiyor.\nEðer gaspý onaylarsaniz ustunuzde {22AF1D}$20.000'{cccccc}den az para varsa tüm parayi alýr.", ReturnName(playerid));
	Dialog_Show(userid, GaspOnay, DIALOG_STYLE_MSGBOX, "Gasp Onayi", strr, "Kabul Et", "Reddet");
	MesajGonder(playerid, "%s adlý oyuncuya gasp istegi gonderildi.", ReturnName(userid));
	return 1;
}

Dialog:GaspOnay(playerid, response, listitem, inputtext[])
{
	if(response)
	{
  		new miktar;
  		if(PlayerData[playerid][pCash] <= 20000) miktar = PlayerData[playerid][pCash];
    	if(PlayerData[playerid][pCash] > 20000) miktar = 20000;
    	ParaVer(PlayerData[playerid][pGaspEden], miktar);
	   	ParaVer(playerid, -miktar);
     	SendNearbyMessage(PlayerData[playerid][pGaspEden], 10.0, COLOR_LIMEGREEN, "{395835}* %s, %s adlý oyuncudan %s miktar para gasp etti!", ReturnName(PlayerData[playerid][pGaspEden]), ReturnName(playerid), FormatNumber(miktar));
		PlayerData[playerid][pGaspEden] = INVALID_PLAYER_ID;
		PlayerData[PlayerData[playerid][pGaspEden]][pGaspSure] = gettime() + 10800;
		PlayerData[playerid][pGaspEdilmeSure] = gettime() + 10800;
		Oyuncu_Kaydet(playerid);
	}
	if(!response)
	{
	    new userid = PlayerData[playerid][pGaspEden];
		MesajGonder(userid, "%s adlý oyuncu sizin gasp isteginizi redetti.", ReturnName(playerid));
		MesajGonder(playerid, "%s adlý oyuncunun gasp istegini redettiniz.", ReturnName(userid));
		PlayerData[playerid][pGaspEden] = INVALID_PLAYER_ID;
	}
	return 1;
}


CMD:saveall(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 7)
	    return HataMesajGonder(playerid, "Yetkiniz yok.");

	new sql = 0;
	if(sscanf(params, "d", sql))
		return HataMesajGonder(playerid, "SQL kayit degeri yazmalisiniz. (1 alir, 0 almaz)");

	if(sql < 0 || sql > 1) return HataMesajGonder(playerid, "SQL kayit degeri yazmalisiniz. (1 alir, 0 almaz)");

	SunucuyuYedekle(sql);
	AdminMessage(COLOR_LIGHTRED, "VERITABANI: %s tarafindan, sunucu yedekleri veritabanina kaydedildi.", ReturnName(playerid, 0));
	return 1;
}

