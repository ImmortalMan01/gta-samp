Dialog:PartiKapat(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		if(GetServerBilgi("oydurum") == 1) return HataMesajGonder(playerid, "Seçimler açýkken parti kapatýlamaz.");
		foreach (new i:Player)
		{
		    if(PlayerData[playerid][pParti] == PlayerData[i][pParti])
		    {
		        if(PlayerData[playerid][pID] != PlayerData[i][pID])
		        {
					MesajGonder(i, "Bulundugunuz parti sahibi tarafindan kapatýldý.");
		            PlayerData[i][pParti] = 0;
		            Oyuncu_Kaydet(i);
		        }
		    }
		}
		new query[256];
		format(query,sizeof(query), "UPDATE oyuncular SET Parti = '0' WHERE Parti = '%d'", PlayerData[playerid][pParti]);
		mysql_query(g_SQL,query);
		new querya[256];
		format(querya,sizeof(querya), "DELETE FROM partiler WHERE Parti = '%d'", PlayerData[playerid][pParti]);
		mysql_query(g_SQL,querya);
		PlayerData[playerid][pParti] = 0;
		MesajGonder(playerid, "Parti kapatýldý.");
		Oyuncu_Kaydet(playerid);
	}
	return 1;
}

