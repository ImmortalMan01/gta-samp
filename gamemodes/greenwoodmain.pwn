CMD:h(playerid, params[])
{
    if (PlayerData[playerid][pAdmin] < 1 && PlayerData[playerid][pHelper] < 1) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok !");
    if (isnull(params)) return BilgiMesajGonder(playerid, "/h [Mesaj]");
    foreach (new i:Player) if ((PlayerData[i][pAdmin] >= 1 || PlayerData[i][pHelper] >= 1) && PlayerData[i][pAyarlar][7] == 0)
    {
        if (strlen(params) > 64)
        {
        	if (PlayerData[playerid][pHelper] > 0) SendClientMessageEx(i, 0x00BBFFFF, "{00BBFF}[Helper Chat] (Seviye: %d) Helper %s:{FFFFFF} %.64s", PlayerData[playerid][pHelper], ReturnName(playerid, 0), params);
        	else SendClientMessageEx(i, 0x00BBFFFF, "{00BBFF}[Helper Chat] %s %s:{FFFFFF} %.64s", GetAdminRank(playerid), PlayerData[playerid][pAdminName], params);
        	SendClientMessageEx(i, 0x00BBFFFF, "%s", params[64]);
        }
        else
        {
