stock trduzelt(mesaj[])
{
	new tmp[1024];
	format(tmp, sizeof(tmp), "%s", mesaj);

	strreplace(tmp, "ð","g");
	strreplace(tmp, "Ð","G");
	strreplace(tmp, "þ","s");
	strreplace(tmp, "Þ","S");
	strreplace(tmp, "ý","i");
	strreplace(tmp, "Ý","I");
	strreplace(tmp, "ö","o");
	strreplace(tmp, "Ö","O");
	strreplace(tmp, "ç","c");
	strreplace(tmp, "Ç","C");
	strreplace(tmp, "ü","u");
	strreplace(tmp, "Ü","U");

  	return tmp;
}

