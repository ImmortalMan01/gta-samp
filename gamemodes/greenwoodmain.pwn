 			if (health < 750) return HataMesajGonder(playerid, "Arac hasarli oldugu için araci park edemiyorsunuz.");
 			if ((vehicleid = Car_GetID(vehicleid)) != -1 && Car_IsOwner(playerid, vehicleid))
 			{
 			    new garajid = Garage_Inside(playerid);
