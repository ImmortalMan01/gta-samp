			MesajGonder(playerid, "Araca siren takildi.");
			SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s elini camdan disari cikarip aracin tavanina siren takar.", ReturnName(playerid, 0));
		}
		case 1:
		{
		    AracInfo[id][aracSirenAcik] = false;

			DestroyDynamicObject(AracInfo[id][SirenObject]);
			MesajGonder(playerid, "Siren kapatýldý.");
			SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s elini camdan disari cikarip aracin tavanindaki sireni cikarir.", ReturnName(playerid, 0));
