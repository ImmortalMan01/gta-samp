Dialog:EditRanks(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    if (!Birlikler[PlayerData[playerid][pFactionEdit]][birlikExists])
			return 0;

		PlayerData[playerid][pSelectedSlot] = listitem;
		Dialog_Show(playerid, SetRankName, DIALOG_STYLE_INPUT, "Rutbe Duzenle", "%d. Rutbe için lütfen yeni bir ad girin:\nÞu anki ad: %s", "Tamam", "{FF6347}Iptal", PlayerData[playerid][pSelectedSlot] + 1, BirlikRutbe[PlayerData[playerid][pFactionEdit]][PlayerData[playerid][pSelectedSlot]]);
	}
	if (!response)
	{
	    PlayerData[playerid][pFactionEdit] = -1;
	    PlayerData[playerid][pSelectedSlot] = -1;
	    cmd_birlik(playerid);

	}
	return 1;
}
