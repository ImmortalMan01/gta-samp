	{
	    case 22: mermi = 200;
	    case 24: mermi = 107;
	    case 25: mermi = 50;
	    case 28: mermi = 200;
	    case 29: mermi = 230;
	    case 30: mermi = 250;
	    case 31: mermi = 250;
	    case 33: mermi = 30;
