CMD:motor(playerid)
{
    new vehicleid = GetPlayerVehicleID(playerid);
    new id = Car_GetID(vehicleid);

    if (!IsEngineVehicle(vehicleid)) return HataMesajGonder(playerid, "Bu komutu sadece aracta kullanabilirsin.");
   	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesajGonder(playerid, "Surucu deðilsin.");
   	if (AracInfo[id][aracBenzin] < 0.1) return HataMesajGonder(playerid, "Bu aracýn yakiti bitmis.");
   	if (AracHasar(vehicleid) <= 300) return HataMesajGonder(playerid, "Bu araca cok fazla hasar verilmis, motor calismiyor.");
   	if (UsingPumpID[playerid] != -1) return HataMesajGonder(playerid, "Þu an motoru calistiramazsiniz!");
	if(SpamArac[playerid] > gettime()) return HataMesajGonder(playerid, "Bu komutu 3 saniyede bir kullanabilirsiniz.");

	if(AracInfo[id][aracPaintjob] == -1) ChangeVehiclePaintjob(PlayerData[playerid][pModifiye], 3);
		else ChangeVehiclePaintjob(PlayerData[playerid][pModifiye], AracInfo[id][aracPaintjob]);

	new col1, col2;
	GetVehicleColor(vehicleid, col1, col2);
	for (new i = 0; i<14; i++)
	{
		if(GetVehicleComponentInSlot(vehicleid, i) != AracInfo[id][aracMods][i])
		{
			RemoveVehicleComponent(vehicleid, GetVehicleComponentInSlot(vehicleid, i));
			if (AracInfo[id][aracMods][i] > 0) AddVehicleComponent(vehicleid, AracInfo[id][aracMods][i]);
			if(CarComponents[vehicleid][i] > 0) CarComponents[vehicleid][i] = 0;
			if(CarComponents[vehicleid][16] != -1) CarComponents[vehicleid][16] = -1;
			if(CarComponents[vehicleid][15] != -1) CarComponents[vehicleid][15] = -1;
			if(CarComponents[vehicleid][14] != -1) CarComponents[vehicleid][14] = -1;
		}
	}
	if(col1 != AracInfo[id][aracRenkler][0] || col2 != AracInfo[id][aracRenkler][1])
	{
		ChangeVehicleColor(vehicleid, AracInfo[id][aracRenkler][0], AracInfo[id][aracRenkler][1]);
	}
   	if (Car_IsOwner(playerid, id) || Car_Birlik(playerid, id) ||  AracInfo[id][aracTip] == 2 || AracInfo[id][aracKiralayan] == PlayerData[playerid][pID] || Arac_AnahtarVar(playerid, id) || (PlayerData[playerid][pFaction] != -1 && GetFactionType(playerid) != BIRLIK_CETE && GetFactionType(playerid) != BIRLIK_MAFYA && GetFactionType(playerid) != BIRLIK_LEGAL && AracInfo[id][aracFactionType] == GetFactionType(playerid)) || (AracInfo[id][aracTip] == 5 && PlayerData[playerid][pVip] > 0))
   	{
   	    switch (GetEngineStatus(vehicleid))
   	    {
   	        case false:
   	        {
   	            MotorGuvenliBolge[vehicleid] = false;
   	            SpamArac[playerid] = gettime() + 3;
   	            SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s anahtari aracýn kontagina sokar ve cevirerek çalýþmasýný bekler.", ReturnName(playerid, 0));
   	            SetTimerEx("MotorCalistir", floatround(AracInfo[id][aracKM]*3), false, "ud", playerid, vehicleid);
   	        }
   	        case true:
   	        {
   	            if(GuvenliBolgedeyse(playerid))
   	            {
   	            	MotorGuvenliBolge[vehicleid] = true;
   	            }
            	SetEngineStatus(vehicleid, false);
            	SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s aracýn anahatarýný cevirerek motoru kapatir.", ReturnName(playerid, 0));
   	        }
   	    }
   	}
   	else
   	{
   	    HataMesajGonder(playerid, "Bu aracýn anahtari sizde bulunmuyor.");
   	}
	return 1;
}

