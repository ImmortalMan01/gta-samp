			cache_delete(VeriCek);

			new queryc[350];
  			format(queryc, sizeof(queryc), "INSERT INTO `oocsicil` (`ID`, `Tarih`, `Sebep`, `Uygulayan`, `Tur`) VALUES('%d', '%s', 'Uyari Asimi', '%s', '5')", sqlid, ReturnDate(), PlayerData[playerid][pAdminName]);
    		mysql_tquery(g_SQL, queryc);

			MesajGonder(playerid, "%s adlý oyuncuya %s sebebiyle uyari yazdiniz.", oyuncuadi, sebep);
			AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý oyuncuya %s sebebiyle uyari yazdi.", PlayerData[playerid][pAdminName], oyuncuadi, sebep);

			new Cache:VeriCek, queryf[256];
			mysql_format(g_SQL, queryf, sizeof(queryf), "SELECT * FROM oyuncular WHERE ID = %d", IsimdenSQLID(oyuncuadi));
