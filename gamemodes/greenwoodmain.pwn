	Modifiye[id][Modifiye_Pos][0] = x;
	Modifiye[id][Modifiye_Pos][1] = y;
	Modifiye[id][Modifiye_Pos][2] = z;
	Modifiye[id][Modifiye_Pos][3] = ang;

	Modifiye[id][Modifiye_Interior] = GetPlayerInterior(playerid);
