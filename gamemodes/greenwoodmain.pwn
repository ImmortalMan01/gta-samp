				}
				if(AracInfo[id][aracModel] != 481 && AracInfo[id][aracModel] != 509 && AracInfo[id][aracModel] != 510)
				{
					GetVehicleParamsEx(vehicle, engine, lights, alarm, doors, bonnet, boot, objective);
