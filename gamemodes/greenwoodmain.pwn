    TogglePlayerControllable(id, true);
    PlayerTextDrawHide(id, YaraliText[id]);
    ClearAnimations(id);
   	PlayerData[id][pDragged] = 0;
	PlayerData[id][pDraggedBy] = INVALID_PLAYER_ID;
	KillTimer(PlayerData[id][pDragTimer]);
