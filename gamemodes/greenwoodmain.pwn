	}
	else
	{
	    PlayerData[id][pVip] = level;
	    PlayerData[id][pVipSure] = gettime()+ (gun*86400);
	    BilgiMesajGonder(id, "%s adlý yetkili size %s paketini %d gunluk verdi. (Bitis tarihi: %s)", PlayerData[playerid][pAdminName], GetVIPName(level), gun, convertDatex(PlayerData[id][pVipSure]));
