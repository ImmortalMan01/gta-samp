forward OnBanAccountDiscord(DCC_User: user, isim[], sure, sebep[50], kanalidsi[DCC_ID_SIZE], adminisim[24], lvl);
public OnBanAccountDiscord(DCC_User: user, isim[], sure, sebep[50], kanalidsi[DCC_ID_SIZE], adminisim[24], lvl)
{
	new rows;
	cache_get_row_count(rows);
	if (rows)
	{
	    new admin, ip[30], sqlid;
	    cache_get_value_name_int(0, "ID", sqlid);
	    cache_get_value_name_int(0, "AdminLevel", admin);
		cache_get_value_name(0, "LastIP", ip, 30);
	    if (admin > lvl) return DCC_SendChannelMessage(DCC_FindChannelById(kanalidsi), "Bu oyuncu sizden yuksek yetkiye sahiptir.");
	    AddBan(ip, isim, "", adminisim, (sure < 1) ? (0) : (gettime()+ (sure*86400)), sebep);
	    new suree[12];
	    if (sure == 0) suree = "sinirsiz";
	    else format(suree, sizeof(suree), "%d gun", sure);
	    new mesajeklendi[128];
	    format(mesajeklendi, sizeof(mesajeklendi), "%s adlý oyuncu %s tarafindan %s sebebiyle offline olarak %s yasaklandi.", isim, adminisim, sebep, suree);
   		DCC_SendChannelMessage(DCC_FindChannelById(kanalidsi), mesajeklendi);

	    AdminMessage(COLOR_LIGHTRED, "AdmLog: (( DC: %s adlý yetkili %s adlý oyuncuyu offline olarak %s sebebiyle %s yasakladi )).", adminisim, isim, sebep, suree);
	    new query[300];
	    format(query, sizeof(query), "INSERT INTO `oocsicil` (`ID`, `Tarih`, `Sebep`, `Uygulayan`, `Tur`) VALUES('%d', '%s', '%s', '%s', '2')",  sqlid, ReturnDate(), sebep, adminisim);
    	mysql_query(g_SQL, query, false);

		new querya[300];
  		format(query,sizeof(query), "UPDATE `oyuncular` SET `ban` = '1' WHERE `ID` = '%d'", sqlid);
    	mysql_query(g_SQL, querya, false);

	    foreach (new i:Player) if (PlayerData[i][pAyarlar][3] == 0)
   		{
    		if (sure == 0) SendClientMessageEx(i, COLOR_LIGHTRED, "(%s) - (( DC: %s adlý oyuncu %s tarafindan %s sebebiyle sunucudan offline sinirsiz yasaklandi. ))", ReturnDate(), isim,  adminisim, sebep);
    		else SendClientMessageEx(i, COLOR_LIGHTRED, "(%s) - (( DC: %s adlý oyuncu %s tarafindan %s sebebiyle sunucudan %d gun offline yasaklandi. ))", ReturnDate(), isim,  adminisim, sebep, sure);
    	}
	}
	else DCC_SendChannelMessage(DCC_FindChannelById(kanalidsi), "Karakter adi bulunamadi.");
	return 1;
}

