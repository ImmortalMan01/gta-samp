Dialog:SirketKasaCek(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		new sirketid = PlayerData[playerid][pSirket];

		if(PlayerData[playerid][pID] == sirketData[sirketid][sirketSahip])
		{
			new deger;

			if(sscanf(inputtext, "d", deger))
				return Dialog_Show(playerid, SirketKasaCek, DIALOG_STYLE_INPUT, "Þirket Kasasi", "Þirket kasasindan cekmek istediginiz miktari giriniz.", "cek", "Kapat");

			if(deger < 1)
				return Dialog_Show(playerid, SirketKasaCek, DIALOG_STYLE_INPUT, "Þirket Kasasi", "Þirket kasasindan cekmek istediginiz miktari giriniz.", "cek", "Kapat");

			if(deger > sirketData[sirketid][sirketKasa])
				return Dialog_Show(playerid, SirketKasaCek, DIALOG_STYLE_INPUT, "Þirket Kasasi", "Þirket kasasindan cekmek istediginiz miktari giriniz.", "cek", "Kapat"), HataMesajGonder(playerid, "Þirket kasasinda yeterli para yok.");

			if(deger > 1000)
			{
				new yenideger = deger * 10/ 100;

				sirketData[sirketid][sirketKasa] -= deger;
				ParaVer(playerid, deger-yenideger);

				Sirket_Kaydet(sirketid);

				MesajGonder(playerid, "Þirket kasasindan $%d cektiniz. (Yuzde 10 kesildi verilen %d)", deger, deger-yenideger);
			}
			else
			{
				sirketData[sirketid][sirketKasa] -= deger;
				ParaVer(playerid, deger);

				Sirket_Kaydet(sirketid);

				MesajGonder(playerid, "Þirket kasasindan $%d cektiniz.", deger);
			}
		}
	}

	return 1;
}

