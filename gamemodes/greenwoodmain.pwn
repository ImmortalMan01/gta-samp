			if(AracInfo[vid][aracBaglandi] == 1) return HataMesajGonder(playerid, "Bu araba zaten hacizli.");
   			AracInfo[vid][aracBaglandi] = 1;
            AracInfo[vid][aracBaglandiCeza] = GetGaleriFiyat(AracInfo[vid][aracModel])/4;
