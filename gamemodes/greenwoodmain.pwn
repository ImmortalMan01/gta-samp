CMD:yakinarabalar(playerid)
{
	if (PlayerData[playerid][pKelepce] > 0 || PlayerData[playerid][pHapisSure] > 0 || PlayerData[playerid][pSoklandi] > 0 || PlayerData[playerid][pYereYatirildi] > 0 || PlayerData[playerid][pBaygin] == 1 || pbOda[playerid] != -1 || GetPVarInt(playerid, "IpBaglandi") == 1)
 		return HataMesajGonder(playerid, "Þu an bu komutu kullanamazsýnýz.");

	new str[1000], kstr[200], Float:vposx, Float:vposy, Float:vposz;
	str = "ID\tPlaka\tModel\n";

	new Float:px, Float:py, Float:pz;
	GetPlayerPos(playerid, px, py, pz);

	for(new i = 1; i < MAX_VEHICLES; i++) // 1'den baþlat
	{
		if(!IsValidVehicle(i)) continue; // Geçersiz araçlarý atla

		GetVehiclePos(i, vposx, vposy, vposz);
		if(!IsPlayerInRangeOfPoint(playerid, 4.0, vposx, vposy, vposz)) continue; // Mesafe kontrolü

		new id = Car_GetID(i);
		if(id < 0 || id >= MAX_ARAC) continue; // Geçersiz ID'leri atla

		format(kstr, sizeof(kstr), "%d\t%s\t%s\n", i, AracInfo[id][aracPlaka], ReturnVehicleModelName(AracInfo[id][aracModel]));
		strcat(str, kstr);
	}

	if(strlen(str) <= 20) // Boþ liste kontrolü
		return HataMesajGonder(playerid, "Yakýnýnda araç bulunamadý!");

	Dialog_Show(playerid, Yakinaracalarnull, DIALOG_STYLE_TABLIST_HEADERS, "Yakýndaki Araçlar", str, "X", "X");
	return 1;
}

