Dialog:KiralikArabaAl(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new aid = strval(inputtext);
		new aaid = Car_GetID(aid);
        if(AracInfo[aid][aracSahip] == -1 && AracInfo[aid][aracKira] > 0 && AracInfo[aid][aracKiralayan] == -1)
        {
            new Float:x, Float:y, Float:z;
            GetVehiclePos(aaid, x, y, z);
        	SetPlayerCheckpoint(playerid, x, y, z, 4.0);
        	MesajGonder(playerid, "Varis noktasi kiralik araba olarak ayarlandi.");
        }
		else KiraMenu(playerid);
	}
	return 1;
}

