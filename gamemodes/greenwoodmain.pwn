		if(sirketData[sirketid][sirketTip] != 3)
		{
			new sorg[120];
			for(new i = 0; i < MAX_PAYLASIM; i++)
			{
			    if(paylasim[i][sirketID] == sirketData[sirketid][sirketID])
			    {
			        paylasim[i][paylasimExists] = 0;
					format(sorg, 120, "DELETE FROM `paylasimlar` WHERE `ID` = '%d'", paylasim[i][ID]);
					mysql_query(g_SQL, sorg);
			    }
			}
		}
        sirketData[sirketid][sirketExists] = false;
        sirketData[sirketid][sirketTip] = 0;
        sirketData[sirketid][sirketID] = 0;
    }
    return 1;
}
