Dialog:SirketKasaKoy(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		new sirketid = PlayerData[playerid][pSirket];

		if(PlayerData[playerid][pID] == sirketData[sirketid][sirketSahip])
		{
			new deger;

			if(sscanf(inputtext, "d", deger))
				return Dialog_Show(playerid, SirketKasaKoy, DIALOG_STYLE_INPUT, "Þirket Kasasi", "Þirket kasasina koymak istediginiz miktari giriniz.", "Ekle", "Kapat");

