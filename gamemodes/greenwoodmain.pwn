stock AracSilahlar(playerid, id)
{
    if (!AracInfo[id][aracExists] || AracInfo[id][aracKilit])
	    return 0;

    static
	    string[256];

	string[0] = 0;

	for (new i = 0; i < 5; i ++)
	{
	    if (22 <= AracInfo[id][aracSilahlar][i] <= 38)
	        format(string, sizeof(string), "%s%s - Mermi: %d\n", string, ReturnWeaponName(AracInfo[id][aracSilahlar][i]), AracInfo[id][aracMermiler][i]);

		else
		    format(string, sizeof(string), "%s%s\n", string, (AracInfo[id][aracSilahlar][i]) ? (ReturnWeaponName(AracInfo[id][aracSilahlar][i])) : ("Bos Slot"));
	}
	if (GetFactionType(playerid) == BIRLIK_LSPD)
		format(string, sizeof(string), "%s{ff1d00}» {FFFFFF}Silahlara El Koy", string);

	Dialog_Show(playerid, BagajSilahlar, DIALOG_STYLE_LIST, "{E74C3C}Bagaj: {FFFFFF}Silahlar", string, "Seç", "Iptal");
	return 1;
}

