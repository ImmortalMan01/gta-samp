CMD:meslek(playerid, params[])
{
	if (isnull(params))
	    return BilgiMesajGonder(playerid, "/meslek [katil/ayril/yardim]");

	if (!strcmp(params, "katil", true))
	{
	    new id = Meslek_Kontrol(playerid);

		if (Meslek[playerid] != 0)
		    return HataMesajGonder(playerid, "Zaten bir meslektesin ilk önce mesleðinden ayril. (\"/meslek ayril\")");

		if (id == -1)
		    return HataMesajGonder(playerid, "Meslek katilma noktasinda deðilsin.");

		Meslek[playerid] = MeslekGiris[id][meslekID];

		MesajGonder(playerid, "Meslege katýldýniz, \"/meslek yardim\" yazarak meslek hakkýnda yardim alabilirsiniz.");
	}
	if (!strcmp(params, "ayril", true))
	{
	    if (Meslek[playerid] == 0)
		    return HataMesajGonder(playerid, "Zaten bir meslekte deðilsin.");

		Meslek[playerid] = 0;

		MesajGonder(playerid, "Meslekten ayrýldýnýz.");
	}
	if (!strcmp(params, "yardim", true))
	{
	    if (Meslek[playerid] == 0)
		    return HataMesajGonder(playerid, "Bu komutu kullanabilmek için bir meslekte olmalýsýn.");

		MeslekYardimGoster(playerid);
