{
	for(new i = 0; i < MAX_ISYERI; i++) if(Isyeri[i][isyeriExists]) Isyeri_Kaydet(i);
	for(new i = 0; i < MAX_ENTRANCES; i++) if(EntranceData[i][entranceExists]) Entrance_Save(i);
	for(new i = 0; i < MAX_EV; i++) if(HouseInfo[i][evExists]) Ev_Kaydet(i);
	for(new i = 0; i < MAX_ARAC; i++) if(AracInfo[i][aracExists]) Arac_Kaydet(i);
	for(new i = 0; i < MAX_BIRLIK; i++) if(Birlikler[i][birlikExists]) Birlik_Kaydet(i);
	for(new i = 0; i < MAX_SIRKET; i++) if(sirketData[i][sirketExists]) Sirket_Kaydet(i);
	for(new i = 0; i < MAX_CCTV; i++) if (Iter_Contains(CCTVIter, i)) SaveCCTV(i);
	for(new i = 0; i < MAX_GARAGE; i++) if(GarageData[i][gExists]) Garaj_Kaydet(i);
	for(new i = 0; i < MAX_GATES; i++) if(GateData[i][gateExists]) Gate_Save(i);
	for(new i = 0; i < MAX_GISE; i++) if(Gise[i][gs_Exists]) Gise_Kaydet(i);
	for(new i = 0; i < MAX_OBJE; i++) if(Obje[i][objeKullanimda]) Obje_Kaydet(i);
    foreach(new i : Player) if(OyundaDegil(i)) Oyuncu_Kaydet(i);

    if(sql == 1)
    {
    	HTTP(0, HTTP_POST, "185.246.113.204/yedek.php", "", "MyHttpResponse");
    }
    return 1;
}

