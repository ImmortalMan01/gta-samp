 			    ParaVer(playerid,-100);
 			    static
					g_arrSeatData[10] = {INVALID_PLAYER_ID, ...},
					g_arrDamage[4],
					seatid;

				GetVehicleDamageStatus(AracInfo[vehicleid][aracVehicle], g_arrDamage[0], g_arrDamage[1], g_arrDamage[2], g_arrDamage[3]);
