		    return Dialog_Show(playerid, PartiUyeAl, DIALOG_STYLE_INPUT, "Parti - üye al", "Üye olarak almak istediginiz kiþinin idsini girin\nBu oyuncu baska bir partide.", "Al", "Iptal");

