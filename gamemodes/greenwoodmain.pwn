Dialog:BJBahis(playerid, response, listitem, inputtext[])
{
    if (!response) return MesajGonder(playerid, "Bahis oranýný belirlemediðin için oyun baþlamadý.");

    new bahis = strval(inputtext);
    if (isnull(inputtext) || bahis < 100 || bahis > 1500)
    {
        return Dialog_Show(playerid, BJBahis, DIALOG_STYLE_INPUT, "{FFFFFF}Kurpiyer - {AA3333}Blackjack",
            "{AA3333}» {FFFFFF}Lütfen geçerli bir bahis miktarý girin ($100 ile $1500 arasýnda olmalýdýr)",
            "Devam", "Geri");
    }

    if (PlayerData[playerid][pCash] < bahis)
    {
        HataMesajGonder(playerid, "Yazdýðýnýz bahis kadar paranýz yok!");
        return Dialog_Show(playerid, BJBahis, DIALOG_STYLE_INPUT, "{FFFFFF}Kurpiyer - {AA3333}Blackjack",
            "{AA3333}» {FFFFFF}Lütfen geçerli bir bahis miktarý girin ($100 ile $1500 arasýnda olmalýdýr)",
            "Devam", "Geri");
    }

    SetPVarInt(playerid, "BahisPara", bahis);
    SetPVarInt(playerid, "OyuncuToplam", 0);
    SetPVarInt(playerid, "KrupiyerToplam", 0);

    Dialog_Show(playerid, BJBahisVerildi, DIALOG_STYLE_LIST, "{FFFFFF}Kumarhane - {AA3333}Blackjack",
        "{FFFFFF}Oyunu Baþlat", "Seç", "Geri");
    return 1;
}

Dialog:BJBahisVerildi(playerid, response, listitem, inputtext[])
{
    if (!response)
    {
        DeletePVar(playerid, "BahisPara");
        return Dialog_Show(playerid, BJBahis, DIALOG_STYLE_INPUT, "{FFFFFF}Kurpiyer - {AA3333}Blackjack",
            "{AA3333}» {FFFFFF}Lütfen oynamak istediðiniz bahis miktarýný girin ($100 ile $1500 arasýnda olmalýdýr)",
            "Devam", "Geri");
    }
    bjdurum = 1;
    new oyuncuKart = RandomEx(1, 13);
    new krupiyerKart = RandomEx(1, 13);

    SetPVarInt(playerid, "OyuncuToplam", oyuncuKart);
    SetPVarInt(playerid, "KrupiyerToplam", krupiyerKart);

    SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* Kurpiyer, kartlari dagitmaya baslar.");
    SendNearbyMessage(playerid, 15.0, COLOR_DO, "* Gelen Kart: %d (( %s ))", oyuncuKart, ReturnName(playerid));
    SendNearbyMessage(playerid, 15.0, COLOR_DO, "* Görünen Kart: %d (( Kurpiyer ))", krupiyerKart);
    SendNearbyMessage(playerid, 15.0, COLOR_WHITE, "Kurpiyer: Kalacak mýsýnýz yoksa çekecek misiniz?");

    Dialog_Show(playerid, BJKalCek, DIALOG_STYLE_LIST, "{FFFFFF}Kumarhane - {AA3333}Blackjack",
        "{FFFFFF}Kal\n{FFFFFF}Çek", "Seç", "Geri");

    return 1;
}

Dialog:BJKalCek(playerid, response, listitem, inputtext[])
{
   	if (!response)
	{
	    Dialog_Show(playerid, BJKalCek, DIALOG_STYLE_LIST, "{FFFFFF}Kumarhane - {AA3333}Blackjack", "{FFFFFF}Kal\n{FFFFFF}Cek", "Seç", "Geri");
	    return 1;
	}
    new oyuncuToplam = GetPVarInt(playerid, "OyuncuToplam");
    new krupiyerToplam = GetPVarInt(playerid, "KrupiyerToplam");

    if (listitem == 0) // Kal
    {
        while (krupiyerToplam < 17)
        {
            krupiyerToplam += RandomEx(1, 13);
        }

        SetPVarInt(playerid, "KrupiyerToplam", krupiyerToplam);

        SendNearbyMessage(playerid, 15.0, COLOR_DO, "* Kurpiyer kart çekiyor...");
        SendNearbyMessage(playerid, 15.0, COLOR_DO, "* Kurpiyerin toplam karti: %d (( Kurpiyer ))", krupiyerToplam);

        if (krupiyerToplam > 21 || oyuncuToplam > krupiyerToplam)
        {
            MesajGonder(playerid, "Tebrikler! %d miktar para kazandiniz.", GetPVarInt(playerid, "BahisPara") * 2);
            ParaVer(playerid,GetPVarInt(playerid, "BahisPara") * 2);
            SendNearbyMessage(playerid, 15.0, COLOR_WHITE, "Kurpiyer: Oynadiginiz icin tesekkurler. Yeni oyun baslayacaktir.");
            bjdurum = 0;
        }
        else if (krupiyerToplam == oyuncuToplam)
        {
            MesajGonder(playerid, "Oyun berabere olarak bitti, para geri iade edildi.");
            ParaVer(playerid, GetPVarInt(playerid, "BahisPara"));
            SendNearbyMessage(playerid, 15.0, COLOR_WHITE, "Kurpiyer: Oynadiginiz icin tesekkurler. Yeni oyun baslayacaktir.");
            bjdurum = 0;
        }
        else
        {
            HataMesajGonder(playerid, "Oyunu kaybettiniz, paraniz kasaya alindi.");
            SendNearbyMessage(playerid, 15.0, COLOR_WHITE, "Kurpiyer: Oynadiginiz icin tesekkurler. Yeni oyun baslayacaktir.");
            bjdurum = 0;
        }
    }
    else if (listitem == 1) // Çek
    {
        new yeniKart = RandomEx(1, 13);
        oyuncuToplam += yeniKart;
        SetPVarInt(playerid, "OyuncuToplam", oyuncuToplam);

        SendNearbyMessage(playerid, 15.0, COLOR_DO, "* Yeni kart: %d (( %s ))", yeniKart, ReturnName(playerid));
        MesajGonder(playerid, "Toplam kartiniz: %d", oyuncuToplam);

        if (oyuncuToplam > 21)
        {
            HataMesajGonder(playerid, "Oyunu kaybettiniz, paraniz kasaya alindi.");
            SendNearbyMessage(playerid, 15.0, COLOR_WHITE, "Kurpiyer: Oynadiginiz icin tesekkurler. Yeni oyun baslayacaktir.");
            bjdurum = 0;
        }
        else
        {
            return Dialog_Show(playerid, BJKalCek, DIALOG_STYLE_LIST, "{FFFFFF}Kumarhane - {AA3333}Blackjack",
                "{FFFFFF}Kal\n{FFFFFF}Çek", "Seç", "Geri");
        }
    }

    DeletePVar(playerid, "BahisPara");
    DeletePVar(playerid, "OyuncuToplam");
    DeletePVar(playerid, "KrupiyerToplam");

    return 1;
}

