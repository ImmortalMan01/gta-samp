	if (IsPlayerConnected(PlayerDrugData[id][DrugsOfferedBy])) return HataMesajGonder(playerid, "Bu kiþiye zaten uyuþturucu satma istegi gonderilmis.");
    if (!(0 < price <= 5000)) return HataMesajGonder(playerid, "Fiyat en az 1, en fazla $7,500 olmalýdýr.");
