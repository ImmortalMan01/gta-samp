DCMD:iade(user, channel, params[])
{
    new DCC_Channel:kanal, kanal_isim[128];
    new DCC_Message:message;
    message = DCMD_GetCommandMessageId();

    new kanalidsi[DCC_ID_SIZE];
    DCC_GetChannelId(channel, kanalidsi);
    DCC_GetMessageChannel(message, kanal);
    DCC_GetChannelName(kanal, kanal_isim);

    // **Belirlenen kanal dýþýnda kullaným engellendi**
    if(strcmp(kanalidsi, "1350589063757037709", true) != 0)
    {
        DCC_SendChannelMessage(channel, "Bu komutu suan kullanamazsin.");
        return 1;
    }

    // **Sunucu ve rol tanýmlamalarý**
    new DCC_Guild:sunucu = DCC_FindGuildById("1344381634480308377"); // Sunucu ID
    new DCC_Role:tmg = DCC_FindRoleById("1352709512716484709"); // Yetkili rol ID

    if(sunucu == DCC_INVALID_GUILD || tmg == DCC_INVALID_ROLE)
    {
        DCC_SendChannelMessage(channel, "Hatali giris yapildi.");
        return 1;
    }

    // **Kullanýcýnýn role sahip olup olmadýðýný kontrol et**
    new bool:Tmgvarmi;
    DCC_HasGuildMemberRole(sunucu, user, tmg, Tmgvarmi);

    if(!Tmgvarmi)
    {
        DCC_SendChannelMessage(channel, "Bu komutu kullanabilmek icin yetkin yeterli degil!");
        return 1;
    }

    if (strfind(kanal_isim, "iade-log", true) != -1)
    {
        new tur[6], karakterad[28], iade;
