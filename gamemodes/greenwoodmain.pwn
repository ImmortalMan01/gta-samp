		GetPVarString(playerid, "Partininadi", partiadi, sizeof partiadi);
		if(sscanf(inputtext, "u", oyuncu))
		    return Dialog_Show(playerid, PartiUyeAl, DIALOG_STYLE_INPUT, "Parti - üye al", "Üye olarak almak istediginiz kiþinin idsini girin\nID girmelisiniz.", "Al", "Iptal");

