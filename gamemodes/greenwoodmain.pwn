CMD:medyadurum(playerid)
{
    if (GetFactionType(playerid) != BIRLIK_GOV) return HataMesajGonder(playerid, "Belediye baskani deðilsiniz.");
    if(twitterdurum == 0 && instadurum == 0)
	{
	    MesajGonder(playerid, "Tum sosyal medyalar erisime kapatildi.");
	    foreach (new i : Player)
		{
	 		SendClientMessageEx(i, 0x2F80C1FF, "Belediye Baskani %s, tum sosyal medya platformlarini kisitladi.", ReturnName(playerid));
	 	}
	    instadurum = 1;
	    twitterdurum = 1;
	    return 1;
	}
	if(twitterdurum == 1 && instadurum == 1)
	{
 		MesajGonder(playerid, "Tum sosyal medyalar erisime tekrardan acildi.");
	    foreach (new i : Player)
		{
	 		SendClientMessageEx(i, 0x2F80C1FF, "Belediye Baskani %s, tum sosyal medya platformlarinin kisitlamasini kaldirdi.", ReturnName(playerid));
	 	}
	    instadurum = 0;
	    twitterdurum = 0;
	    return 1;
	}
	return 1;
}

