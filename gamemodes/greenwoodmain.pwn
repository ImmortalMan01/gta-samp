    			}
                Birlikler[PlayerData[playerid][pFaction]][birlikKasaPara] -= Birlikler[PlayerData[playerid][pFaction]][cekilisOdul];
                Birlikler[PlayerData[playerid][pFaction]][CekilisBasladi] = false;
