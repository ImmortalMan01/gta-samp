{
    if (sirketid != -1 && sirketData[sirketid][sirketExists])
    {
        new
            string[64];

        format(string, sizeof(string), "DELETE FROM `sirketler` WHERE `sirketID` = '%d'", sirketData[sirketid][sirketID]);
