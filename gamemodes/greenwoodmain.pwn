    static query[1100];

		format(query, sizeof(query), "UPDATE `entrances` SET `entranceName` = '%s', `entranceInteriorVw` = '%d', `entrancePass` = '%s', `entranceIcon` = '%d', `entranceLocked` = '%d', `entrancePosX` = '%.4f', `entrancePosY` = '%.4f', `entrancePosZ` = '%.4f', `entrancePosA` = '%.4f'",
