Dialog:PozisyonMaas(playerid, response, listitem, inputtext[]) {

    if(response)
    {
    	new maas;

    	if(sscanf(inputtext, "d", maas))
    		return HataMesajGonder(playerid, "Geçersiz maas miktari girdiniz.");

    	if(maas < 50 || maas > 500)
    		return HataMesajGonder(playerid, "Maas en az $50, en fazla $500 olabilir.");

    	new sirketid = PlayerData[playerid][pSirket];
		sirketMaaslar[sirketid][GetPVarInt(playerid, "SecilenPoz")] = maas;
		new sayi = 0;
		foreach(new i : Player)
		{
			if(PlayerData[i][pSirket] != sirketid)
				continue;

			if(PlayerData[i][pSirketRank] != GetPVarInt(playerid, "SecilenPoz"))
				continue;

			PlayerData[i][pSirketMaas] = maas;
			sayi++;
		}
		if(sayi == 0)
		{
	        new query[256];
	        format(query, sizeof(query), "UPDATE oyuncular SET SirketMaas=%d WHERE ID=%d", maas, sirketCalisanlar[sirketid][GetPVarInt(playerid, "SecilenPoz")]);
	        mysql_query(g_SQL, query, false);
		}
    }
    return 1;
}
Dialog:SirketAyril(playerid,response,listitem,inputtext[])
