CMD:yemver(playerid, params[])
{
	new id, YemTuru, YemMiktar;

    if (PlayerData[playerid][pAdmin] < 5)
		return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok !");

	if (sscanf(params, "udd", id, YemTuru, YemMiktar))
	    return BilgiMesajGonder(playerid, "/yemver [Oyuncu Isim/ID] [2 = Karides | 1 = Sulunes | 0 = Ekmek Kirintisi] [Miktar]");

	if (YemTuru < 0 || YemTuru > 2)
	    return HataMesajGonder(playerid, "Yem Turune maksimum 3, minimum 1 yazabilirsin.");

	if (YemMiktar < 1 || YemMiktar > 30)
	    return HataMesajGonder(playerid, "Yem miktari minimum 1, maksimum 30 olabilir.");

	PlayerData[id][pYemTurleri][YemTuru] += YemMiktar;

	switch (YemTuru)
	{
		case 0:
		{
			BilgiMesajGonder(playerid, "%s isimli oyuncuya %d tane Ekmek Kýrýntýsý yemi verdiniz.", ReturnName(id, 0), YemMiktar);
			BilgiMesajGonder(id, "%s isimli yetkiili size %d tane Ekmek Kýrýntýsý yemi verdi.", ReturnName(playerid, 0), YemMiktar);
		}
        case 1:
		{
			BilgiMesajGonder(playerid, "%s isimli oyuncuya %d tane Sulunes yemi verdiniz.", ReturnName(id, 0), YemMiktar);
			BilgiMesajGonder(id, "%s isimli yetkiili size %d tane Sulunes yemi verdi.", ReturnName(playerid, 0), YemMiktar);
		}
		case 2:
		{
			BilgiMesajGonder(playerid, "%s isimli oyuncuya %d tane Karides yemi verdiniz.", ReturnName(id, 0), YemMiktar);
			BilgiMesajGonder(id, "%s isimli yetkiili size %d tane Karides yemi verdi.", ReturnName(playerid, 0), YemMiktar);
		}
	}
	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþiye %d tane yem verdi.", PlayerData[playerid][pAdminName], ReturnName(id, 0), YemMiktar);
	return 1;
}

