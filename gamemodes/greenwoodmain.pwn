	}
	if (GetPVarInt(playerid, "OdunGotur") == 1)
	{
	    DisablePlayerCheckpoint(playerid);
	    MesajGonder(playerid, "Odunlari aractan \"/odun aractan al\" yazarak alabilir, Oduncu'ya satabilirsiniz.");
	}
	if (PlayerData[playerid][pTaksiKabul] != -1)
	{
	    PlayerData[playerid][pTaksiKabul] = -1;
