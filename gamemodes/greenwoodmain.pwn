		}
	}
	return 1;
}

forward Float:GetDistanceBetweenPlayers(p1,p2); public Float:GetDistanceBetweenPlayers(p1,p2){ new Float:x1,Float:y1,Float:z1,Float:x2,Float:y2,Float:z2; if (!IsPlayerConnected(p1) || !IsPlayerConnected(p2)){ return -1.00; } GetPlayerPos(p1,x1,y1,z1); GetPlayerPos(p2,x2,y2,z2); return floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2)); }

public OnPlayerDamage(&playerid, &Float:amount, &issuerid, &weapon, &bodypart)
{
	if (weapon == WEAPON_CARPARK || weapon == WEAPON_HELIBLADES || weapon == WEAPON_CHAINSAW || weapon == WEAPON_MINIGUN || weapon == WEAPON_ROCKETLAUNCHER) return 0;
    if(weapon == 16 || weapon == 18 || weapon == 35 || weapon == 36 || weapon == 37 || weapon == 39) return 0;
    if(weapon == 4 && GetDistanceBetweenPlayers(playerid, issuerid) > 2) return 0;

	if (GetPVarInt(playerid, "KOSKorumasi") == 1) return 0;
	if (PlayerData[playerid][pGOD] == true && PlayerData[playerid][pAdmin] > 0) return 0;
	if (PlayerData[playerid][pBaygin] == 1 && !IsBulletWeapon(weapon)) return 0;
	if (IsPlayerPause(playerid)) return 0;
	new SilahSlot = g_aWeaponSlots[GetPlayerWeapon(playerid)], SilahVerileri[1][2];
	GetPlayerWeaponData(playerid, SilahSlot, SilahVerileri[0][0], SilahVerileri[0][1]);
	if (GetPlayerWeapon(playerid) != 0)
	{
  		if (SilahVerileri[0][1] != PlayerData[playerid][pMermiler][g_aWeaponSlots[GetPlayerWeapon(playerid)]])
		{
			AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s ile mermi hilesi kullanmaya calisiyor. (damage)", ReturnName(playerid), ReturnWeaponName(weapon));
			SetWeapons(playerid);
	        return false;
	    }
	}
	if (issuerid != INVALID_PLAYER_ID)
	{
		if (PlayerData[issuerid][pBaygin] == 1) return 0;
	}

	if (issuerid != INVALID_PLAYER_ID)
	{
		if (PlayerData[issuerid][pProbation] == 1)
		{
	 		SetPlayerArmedWeapon(issuerid, 0);
		    Dialog_Show(issuerid, Probationates, DIALOG_STYLE_MSGBOX, "{FF0000}Probation", "{FFFFFF}Probationdaki insanlar ates edemezler.", "Tamam", "");
		    return 0;
		}
	}

	if (issuerid != INVALID_PLAYER_ID)
	{
		if(IsPlayerInAnyVehicle(issuerid))
		{
			if(IsBulletWeapon(weapon))
			{
 				AdminMessage(COLOR_LIGHTRED, "AdmLog: %s adlý oyuncu arabadayken hasar vermeye calisti, arabadan atýldý. (damage)", ReturnName(issuerid));
 				RemovePlayerFromVehicle(issuerid);
 				Dialog_Show(issuerid, ArabaDriveBy, DIALOG_STYLE_MSGBOX, "Uyari", "Arabadayken hasar veremezsiniz.", "Tamam", "");
			}
		}
	}

    if (IsPlayerInAnyVehicle(playerid) && weapon == 9)
	    return 0;

	if (IsPlayerInAnyVehicle(playerid))
	{
	    new id = Car_GetID(GetPlayerVehicleID(playerid));
	    if (id != -1 && AracInfo[id][aracZirh] > 75)
	    {
	        AracInfo[id][aracZirh] -= 50;
	        return 0;
	    }
	}

	if (issuerid != INVALID_PLAYER_ID)
	{
    	if (PlayerData[playerid][pLevel] < 3)
    	{
			SetPlayerArmedWeapon(issuerid, 0);
			Dialog_Show(issuerid, GBIUyarialmak, DIALOG_STYLE_MSGBOX, "Uyari", "3 Seviye alti oyuncular hasar alamaz.", "Tamam", "");
			return 0;
    	}
	}

    if (issuerid != INVALID_PLAYER_ID)
    {
    	if (PlayerData[issuerid][pLevel] < 3)
    	{
			SetPlayerArmedWeapon(issuerid, 0);
			Dialog_Show(issuerid, GBIUyarivermek, DIALOG_STYLE_MSGBOX, "Uyari", "3 Seviye alti oyuncular hasar veremez.", "Tamam", "");
			return 0;
    	}
	}

	if (issuerid != INVALID_PLAYER_ID)
	{
 		if (GuvenliBolgedeyse(issuerid) && GetFactionType(issuerid) != BIRLIK_LSPD)
		{
 			SetPlayerArmedWeapon(issuerid, 0);
 			Dialog_Show(issuerid, GBIUyari, DIALOG_STYLE_MSGBOX, "{FF0000}Kisi guvenli bolgede!", "{FFFFFF}Ates ettiginiz kiþi guvenli bolgede oldugu için hasar veremezsiniz.", "Tamam", "");
 			return 0;
		}
	}

	if (playerid != INVALID_PLAYER_ID)
	{
	    if (GuvenliBolgedeyse(playerid) && GetFactionType(playerid) != BIRLIK_LSPD)
		{
		    SetPlayerArmedWeapon(playerid, 0);
		    Dialog_Show(playerid, GBIUyari, DIALOG_STYLE_MSGBOX, "{FF0000}Guvenli Bolgedesiniz!", "{FFFFFF}Bulundugunuz bolge guvenli bolgedir, hasar veremezsiniz.", "Tamam", "");
		    return 0;
		}
	}

    if (GetPlayerWeapon(playerid) >= 1 && GetPlayerWeapon(playerid) <= 45 && PlayerData[playerid][pSilahlar][g_aWeaponSlots[GetPlayerWeapon(playerid)]] != GetPlayerWeapon(playerid) && !PlayerHasTazer(playerid) && !PlayerHasBeanbag(playerid))
    {
