	    return 0;
	}
	if (weaponid > 0 && GetPVarInt(playerid, "CCTVKontrol") < gettime())
	{
	    new id = GetNearestCCTV(playerid);
	    SetPVarInt(playerid, "CCTVKontrol", gettime()+10);
