CMD:kuponekle(playerid, params[])
{
    if (!OyundaDegil(playerid)) return 1;
    if (PlayerData[playerid][pAdmin] < 8) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok!");
	new kod[20], para;
	if (sscanf(params, "is[20]", para, kod)) return BilgiMesajGonder(playerid, "/kuponekle [Para] [Kod]");
	new query[128];
	mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM `kuponlar` WHERE `kod` = '%e'", kod);
	new Cache:Radless = mysql_query(g_SQL, query);
	if (cache_num_rows())
	{
		HataMesajGonder(playerid, "Bu kupon kodu zaten bulunuyor.");
	}
	else
	{
		mysql_format(g_SQL, query, sizeof(query), "INSERT INTO `kuponlar` (`kod`, `para`) VALUES ('%e', '%d')", kod, para);
		mysql_tquery(g_SQL, query);
		MesajGonder(playerid, "Kupon kodu oluþturuldu.");
	}
	cache_delete(Radless);
	return 1;
}
CMD:kuponsil(playerid, params[])
{
    if (!OyundaDegil(playerid)) return 1;
    if (PlayerData[playerid][pAdmin] < 8) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok!");
	new kod[20];
	if (sscanf(params, "s[20]", kod)) return BilgiMesajGonder(playerid, "/kuponsil [Kod]");
	new query[128];
	mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM `kuponlar` WHERE `kod` = '%e'", kod);
	new Cache:Radless = mysql_query(g_SQL, query);
	if (cache_num_rows())
	{
		mysql_format(g_SQL, query, sizeof(query), "DELETE FROM `kuponlar` WHERE `kod` = '%e'", kod);
		mysql_tquery(g_SQL, query);
		MesajGonder(playerid, "Hediye kuponu silindi.");
	}else
	{
		HataMesajGonder(playerid, "Böyle bir kod yok.");
	}
	cache_delete(Radless);
	return 1;
}
CMD:setadmin(playerid,params[])
{
	new id,level;
	if (!OyundaDegil(playerid)) return 1;
    if (PlayerData[playerid][pAdmin] < 7) return HataMesajGonder(playerid, "Bu komutu kullanmak için yetkiniz yok!");
    if (sscanf(params, "ui",id,level)) return BilgiMesajGonder(playerid, "/setadmin [ID/Isim] [Level]");
    if (!OyundaDegil(id)) return HataMesajGonder(playerid, "Oyuncu giris yapmamis.");
    if (level < 0 || level > 7) return HataMesajGonder(playerid, "Level 0'dan küçük, 7'den fazla olamaz.");
    SendAdminAction(id, "%s adlý yetkili tarafindan %d seviye yetkili oldunuz.",PlayerData[playerid][pAdminName],level);
    SendAdminAction(playerid, "%s adlý oyuncuyu %d seviye yetkili yaptiniz.",ReturnName(id),level);
    if (level == 0)
    {
        if (PlayerData[id][pAdminDuty] == 1)
        {
            cmd_aduty(id);
        }
	}
    PlayerData[id][pAdmin] = level;
    PlayerData[id][pHelper] = 0;
	AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %s adlý kiþiyi %d seviye yetkili yapti.", PlayerData[playerid][pAdminName], ReturnName(id, 0), level);
    Log_Write("logs/admin-log.log", "[%s] %s adlý Admin %s adlý oyuncuyu %d seviye yetkili yapti.",ReturnDate(),ReturnName(playerid),ReturnName(id),level);
	return 1;
}

