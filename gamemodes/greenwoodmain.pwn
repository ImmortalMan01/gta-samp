                if (Birlikler[PlayerData[playerid][pFaction]][cekilisOdul] < 5000 || Birlikler[PlayerData[playerid][pFaction]][birlikKasaPara] < Birlikler[PlayerData[playerid][pFaction]][cekilisOdul])
