stock SoyadKilitlimi(soyad[])
{
    new query[125], Cache: owner_query;
    mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM `kilitlisoyadlar` WHERE `Soyad` = '%s'", SQL_ReturnEscaped(soyad));
