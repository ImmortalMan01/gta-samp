	    else AdminMessage(COLOR_LIGHTRED, "AdmLog: (( %s adlý yetkili %s adlý oyuncuyu %s sebebiyle %d gun yasakladi.", PlayerData[playerid][pAdminName], ReturnName(id, 0), sebep, gun);
	    Log_Write("logs/ban_log.txt", "[%s] %s,  %s tarafindan %d gun yasaklandi. sebep: %s.", ReturnDate(), ReturnName(id, 0), PlayerData[playerid][pAdminName], gun, sebep);
	    new suree;
	    if (gun < 1) suree = 0;
	    else suree = gettime()+ (gun*86400);
	    AddBan(pip, Player_GetName(id), "", PlayerData[playerid][pAdminName], suree, sebep);
	    Kick(id);
	    new DCC_Channel:kanal, kanal_isim[128];
 		new DCC_Message:message;
  		new str[300], gonderen[64];
		format(str, sizeof(str), ":bangbang: AdmLog: (%s) %s tarafindan %s sunucudan yasaklandi. sebep: %s.", ReturnDate(), PlayerData[playerid][pAdminName], ReturnName(id, 0), sebep);
		DCC_SendChannelMessage(DCC_FindChannelById("1352669282395230279"), str);
		return 1;
	}
	if (!IsPlayerConnected(oyuncuid))
	{
	    new query[124];
    	format(query, sizeof(query), "SELECT * FROM `oyuncular` WHERE `Isim` = '%s'", ad);
    	mysql_tquery(g_SQL, query, "OnBanAccount", "isis", playerid, ad, gun, sebep);
