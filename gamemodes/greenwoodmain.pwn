			cache_get_value_name_int(0, "Uyari", uyariadet);
			if(uyariadet >= 3)
			{
				new pip[32];
				cache_get_value_name(0, "LastIP", pip, 32);
			    new querya[256];
				mysql_format(g_SQL, querya, sizeof(querya), "UPDATE oyuncular SET Uyari=0 WHERE ID = '%d'", IsimdenSQLID(oyuncuadi));
				mysql_query(g_SQL, querya);

	   			new queryb[350];
		    	format(query, sizeof(queryb), "INSERT INTO `oocsicil` (`ID`, `Tarih`, `Sebep`, `Uygulayan`, `Tur`) VALUES('%d', '%s', 'Uyari Asimi', '%s', '2')", sqlid, ReturnDate(), PlayerData[playerid][pAdminName]);
		    	mysql_tquery(g_SQL, queryb);
		    	foreach (new i:Player) if (PlayerData[i][pAyarlar][3] == 0)
		    	{
		    		SendClientMessageEx(i, COLOR_LIGHTRED, "(%s) - (( %s adlý oyuncu %s tarafindan uyari asimi sebebiyle sunucudan 3 gun offline yasaklandi. ))", ReturnDate(), oyuncuadi,  PlayerData[playerid][pAdminName]);
		    	}
		    	AdminMessage(COLOR_LIGHTRED, "AdmLog: (( %s adlý yetkili %s adlý oyuncuyu uyari asimi sebebiyle 3 gun offline yasakladi. ))", PlayerData[playerid][pAdminName], oyuncuadi);
		    	Log_Write("logs/ban_log.txt", "[%s] %s,  %s tarafindan 3 gun offline yasaklandi. sebep: uyari asimi", ReturnDate(), oyuncuadi, PlayerData[playerid][pAdminName]);
		    	new suree = gettime()+ (3*86400);
		    	AddBan(pip, oyuncuadi, "", PlayerData[playerid][pAdminName], suree, "Uyari Asimi");
			}
			cache_delete(VeriCek);

		}
		else
		{
		    HataMesajGonder(playerid, "Böyle bir oyuncu yok.");
		    cache_delete(VeriCek);
		}
	}
	return 1;
}

