	            HataMesajGonder(playerid, "Geçersiz arac modeli girdiniz.");
	            return Dialog_Show(playerid,AracEkle,DIALOG_STYLE_INPUT, "{2ECC71}Arac Ekle", "Lütfen iþyerine eklemek istediginiz aracýn adini veya modelini girin:", "Devam", "Kapat");
