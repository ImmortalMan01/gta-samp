		AdminMessage(COLOR_LIGHTRED, "AdmLog: %s, %d idli tamirhanenin birlik idsi %d olarak ayarladi.", PlayerData[playerid][pAdminName], id, birlik);
		Tamirhane_Kaydet(id, true);
		return 1;
	}
	cmd_tamirduzenle(playerid, "");
	return 1;
}

